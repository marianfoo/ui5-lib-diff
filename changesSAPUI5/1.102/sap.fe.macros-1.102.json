{
    "1.102.0": {
        "date": "May 2022",
        "notes": [
            {
                "id": "f2ba914b52fdd21bfe07e1d78682ef189c6b6c36",
                "type": "FIX",
                "text": " When adding a column through the personalisation the semantic link now is correctly displayed",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "002075129400003183552022"
                    }
                ]
            },
            {
                "id": "0baec91aba78b624305ad4f8902ace1c5728704e",
                "type": "FIX",
                "text": " We're now using internationalization on Boolean properties to ensure correct translation when exporting a table",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2170301935"
                    }
                ]
            },
            {
                "id": "3eb1911e8c74a065c15391763340cb6fc8204833",
                "type": "FEATURE",
                "text": " You can now enable keyboard shortcuts for custom actions in your application"
            },
            {
                "id": "2907cf6ca8dc5e1950797ba975c3ad8172ba3247",
                "type": "FIX",
                "text": " We've fixed the filename path for 'Composition of one'",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270078842"
                    }
                ]
            },
            {
                "id": "2769771c4c63f3760bc76b511d6d5072394d8e41",
                "type": "FEATURE",
                "text": " SAP Fiori elements now displays an icon for Situation Handling"
            },
            {
                "id": "351415b016c7f39f2d3b13218bf4a739e16ba308",
                "type": "FEATURE",
                "text": " You can now export the content of a table as a PDF file"
            },
            {
                "id": "6cba6dad0086e741eb5b1b8b1fb9ec9e23450a6b",
                "type": "FEATURE",
                "text": " The 'Form' building block now supports the FieldGroup annotation"
            },
            {
                "id": "e514df4825d5e7da00712b9cd2842cf0937b22ca",
                "type": "FIX",
                "text": " Non-nullable properties are now correctly supported in fields",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270044357"
                    }
                ]
            },
            {
                "id": "c580043008c191b49c71e4471b0056006c14417f",
                "type": "FIX",
                "text": " The UI.hidden annotation now works correctly for the 'contact' field",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "002075129500002877402022"
                    }
                ]
            },
            {
                "id": "7ab53797b11a9dd9892302df7abd05bc91a4df8b",
                "type": "FIX",
                "text": " Additional columns in the value help table are now hidden if certain preconditions are fulfilled BCP: 2270064454"
            },
            {
                "id": "e752438de57f91d683f5cfe3cb2ffc2b8b7a83f0",
                "type": "FEATURE",
                "text": " Personalization changes of chart are now stored/restored from iAppstate"
            },
            {
                "id": "67279ed19daf3f250d54c2d99195a183deb62e01",
                "type": "FIX",
                "text": " The Timezone property for the DateTimeOffset is now correctly displayed in tables",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270064756"
                    },
                    {
                        "type": "BCP",
                        "reference": "2270056209"
                    }
                ]
            },
            {
                "id": "1b074378b7e70c1572962077c8a6f6f1f5a1fdef",
                "type": "FIX",
                "text": " SAP Fiori elements now provides the correct label for filter fields defined in the manifest.json",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280080378"
                    }
                ]
            },
            {
                "id": "402842f74fcc1e2cea5a6103cdf00dbbace0c39f",
                "type": "FEATURE",
                "text": " SAP Fiori elements now supports the synchronization of data between pages when using the flexible column layout"
            },
            {
                "id": "f459985a496b18734dff66b8cbead3ef36010446",
                "type": "FEATURE",
                "text": " We've enhanced the 'Action' building block to support the 'enabled' property"
            },
            {
                "id": "83248108f1d49b985491e8f4b391b3cf383140d6",
                "type": "FIX",
                "text": " A link in the title of a quick view now contains the correct parameters",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270067985"
                    }
                ]
            },
            {
                "id": "4c15d163b1274f409127d3abe99e6babb0e1ffb3",
                "type": "FIX",
                "text": " A new table row is now always added at the beginning of a table"
            },
            {
                "id": "f0c81111a8e0f0320494e221df97397dbaad7da5",
                "type": "FIX",
                "text": " We've fixed an issue that resulted in a table not loading correctly",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270067161"
                    }
                ]
            },
            {
                "id": "fa9093eafb4bd2dd3451b9298bbc21fae9a3142a",
                "type": "FIX",
                "text": " Exclude operators now work if they are not part of the selection",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270019109"
                    }
                ]
            },
            {
                "id": "98b47ad53be0af11067ff6ba21b81900af15e32d",
                "type": "FIX",
                "text": " We've fixed the behavior of the Delete button after a user made changes in the table personalization dialog using the Settings button",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270042334"
                    }
                ]
            },
            {
                "id": "c3173708796c028cb227794fddd09a36b337039c",
                "type": "FEATURE",
                "text": " You can now format a DataPoint of type DateTimeOffset with a specific timezone"
            },
            {
                "id": "50776aabd1126f4afdafe8a88391b59620772f39",
                "type": "FIX",
                "text": " We've added the closeOverflowOnInteraction attribute to correctly render the 'Share' menu",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280029671"
                    }
                ]
            },
            {
                "id": "ab7906d04606c6f7800fe1694bd7612797a09fe1",
                "type": "FIX",
                "text": " SAP Fiori elements now supports internalization on the quick filters"
            },
            {
                "id": "b54fc685599135745832cbda1ab7ff0fcf793a82",
                "type": "FIX",
                "text": " SAP Fiori elements now correctly computes the width of columns based on the DataField",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270049327"
                    }
                ]
            },
            {
                "id": "a7107c7ef36663f9727fb3423750c3c792eebbfb",
                "type": "FIX",
                "text": " We've resolved the console error related to a duplicate ID for the TableValueHelp",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270053364"
                    },
                    {
                        "type": "BCP",
                        "reference": "2270018191"
                    }
                ]
            },
            {
                "id": "a3da55fba80eed8f5072363c2d8937412963c572",
                "type": "FIX",
                "text": " We've made some corrections to our custom filter support",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2180410603"
                    }
                ]
            },
            {
                "id": "82c4a0d1bd3abcec8c39e91114e3173651674121",
                "type": "FIX",
                "text": " SAP Fiori elements now enables you to add a column with a value help dialog to a table"
            },
            {
                "id": "4b07274f528c1ebbe5cebe033a947b6e685c048b",
                "type": "FIX",
                "text": " SAP Fiori elements now correctly displays the DataPoint if its unit is set to %",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270052624"
                    }
                ]
            },
            {
                "id": "701ec772a54d13cc110b926a5023919ab5a014cf",
                "type": "FIX",
                "text": " Semantic object mapping now works correctly for semantic links",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270023425"
                    }
                ]
            },
            {
                "id": "412f957ffe33e3f066fce7b2bf6ee89ddb8e55be",
                "type": "FEATURE",
                "text": " You can now format the DateTimeOffset with a specified timezone"
            },
            {
                "id": "91002ec15e4559d3e60221ab20996bb60cd265b1",
                "type": "FEATURE",
                "text": " V4:FE In SAP Fiori elements for OData V4, you can now use the new dynamic date range control for DateTime fields in the filter bar"
            },
            {
                "id": "51d0928a20611d147f0a4e084541ed48a6d91426",
                "type": "FEATURE",
                "text": " SAP Fiori elements for OData V4 now supports charts based on draft-enabled entity sets"
            },
            {
                "id": "05c6c3af1b381a4a8226f43fe2b849e6c289ac36",
                "type": "FIX",
                "text": " The focus is now set correctly on the cell of a table also in full-screen mode",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270029852"
                    }
                ]
            },
            {
                "id": "6d46eff7dd3df56dc6138df9a4e7721ce00e3b1b",
                "type": "FIX",
                "text": " Draft handling is now processed correctly using flexible column layout when navigating to another object in the list report",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270039446"
                    }
                ]
            },
            {
                "id": "6c50242e61584119adf52c4b6a99b9fef504965a",
                "type": "FEATURE",
                "text": " The filter bar of the value help dialog is now expanded by default under certain conditions"
            },
            {
                "id": "d862c88665e2b6b6ae5a6f88718f79c6f9aaf298",
                "type": "FIX",
                "text": " The visibility of the FileWrapper is now correctly set in the ObjectPage",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270029114"
                    }
                ]
            },
            {
                "id": "2ce037fbeb049358a5b650d6c52713ce45f26d53",
                "type": "FIX",
                "text": " Quick views now contain the correct navigation targets with correct parameters",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280067674"
                    }
                ]
            },
            {
                "id": "5aaebabac1cf4450a00f4996191c881826900b03",
                "type": "FIX",
                "text": " In a custom fragment, adding or removing a column from a table now works correctly",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270015314"
                    }
                ]
            },
            {
                "id": "1664ac0bcbdcba8b484a16602b6541f6422fb34e",
                "type": "FIX",
                "text": " Quick views now contain the correct navigation targets",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280067674"
                    }
                ]
            },
            {
                "id": "fdec946e3b9c05f6e3a3dee3620260f753857752",
                "type": "FIX",
                "text": " SAP Fiori elements for OData V4 now provides filtering on tables on the object page by default"
            },
            {
                "id": "96252644d9c96ee57323888ff828ede04268a7db",
                "type": "FEATURE",
                "text": " We've enhanced the 'Table' building block to support the isSearchable property"
            },
            {
                "id": "1db50713eb6f1d6c0044879d342e26674f006a40",
                "type": "FIX",
                "text": " The FieldControl annotations within ConnectedFields are now taken into account to indicate mandatory fields",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270013538"
                    }
                ]
            },
            {
                "id": "67a069a30fbf477eaead6414b54419eed5d67c48",
                "type": "FEATURE",
                "text": " SAP Fiori elements now supports the property 'Importance' for building block columns"
            },
            {
                "id": "86995e970c96bcceaaf617cda6c850eba17ff042",
                "type": "FEATURE",
                "text": " The UI.Importance of a newly added column is now properly evaluated"
            },
            {
                "id": "8be914258482a8e0c4202f884e7b1686fda7021b",
                "type": "FIX",
                "text": " The DraftIndicator status is now displayed as the last element in a FieldGroup"
            }
        ]
    },
    "1.102.1": {
        "date": "June 2022",
        "notes": [
            {
                "id": "585724a24742a11f8481289ddac4ca6a785696c4",
                "type": "FIX",
                "text": " Situation Handling now works in all tables as well as in entity sets with parameters",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280093857"
                    }
                ]
            },
            {
                "id": "4f68d94a52b27fb9f6cbc6ae447a52b1827e1260",
                "type": "FIX",
                "text": " A semantic object now correctly handles a path in its PropertyValue",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270088070"
                    },
                    {
                        "type": "BCP",
                        "reference": "2280086993"
                    }
                ]
            },
            {
                "id": "df609d91bd3f13907f5b5f640be0538f0044e2b0",
                "type": "FIX",
                "text": " SAP Fiori elements now correctly determines the DisplayMode for a FilterField",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280084870"
                    }
                ]
            },
            {
                "id": "39aaa1b4ca6472ffc150957b9365c4730d371faf",
                "type": "FIX",
                "text": " The QuickView now uses the correct log API",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280088526"
                    }
                ]
            },
            {
                "id": "43d732799e2cb149c413b5703f8ebd1701068b06",
                "type": "FIX",
                "text": " SAP Fiori elements now correctly uses case-insensitive search when a user types in input using the value help",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270098468"
                    }
                ]
            },
            {
                "id": "3ba6de1791712fa6fb8d10142202a9c80a257058",
                "type": "FIX",
                "text": " Connected fields are now rendered properly in edit mode",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "002075129400003401382022"
                    }
                ]
            },
            {
                "id": "6f09aa9a2b26b2af23e8db417440a44f87f4f2bd",
                "type": "FIX",
                "text": " The situations preview dialog no longer fails to show decimal values",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270105689"
                    }
                ]
            },
            {
                "id": "2c477fb19c6391b9cf0e6cf8a270a1f70f46122b",
                "type": "FIX",
                "text": " We're no longer showing a locked indicator in a user's own draft",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270072948"
                    }
                ]
            }
        ]
    },
    "1.102.2": {
        "date": "July 2022",
        "notes": [
            {
                "id": "6c87929d08db8d1552e677318c2250aa3a893730",
                "type": "FIX",
                "text": " The DTS file generation for sap.fe now contains the correct code",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280118606"
                    }
                ]
            },
            {
                "id": "6ac01801981fb10d104e15879cd2f38cbf94e322",
                "type": "FIX",
                "text": " The labels of connected fields can now be read by the screen reader",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270037966"
                    }
                ]
            }
        ]
    },
    "1.102.3": {
        "date": "July 2022",
        "notes": [
            {
                "id": "f9018f9365843fa6e661e66945dba25559498a76",
                "type": "FIX",
                "text": " We've removed the 'undefined' property from a fragment part",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280114697"
                    }
                ]
            }
        ]
    },
    "1.102.4": {
        "date": "August 2022",
        "notes": [
            {
                "id": "6a102c558f80532495c289b1a9ed08ffd3122dc1",
                "type": "FIX",
                "text": " We now prevent unnecessary $batch calls when opening a value help",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280157161"
                    }
                ]
            },
            {
                "id": "6cca4db5280365c864b5a8bb3f6bc5b190f5e889",
                "type": "FIX",
                "text": " The out parameters of a value list in the table settings dialog are now ignored",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280141804"
                    }
                ]
            },
            {
                "id": "4f951e52a944fca177120dd8e3f99d8883fcaacd",
                "type": "FIX",
                "text": " We now read default values of parameters of unbound actions",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "002075129500005127072022"
                    }
                ]
            },
            {
                "id": "7a31d666e08dd5f810c70f2ff37cbf3739c8078a",
                "type": "FIX",
                "text": " Sorting of Chart in OP works properly",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280118444"
                    }
                ]
            }
        ]
    },
    "1.102.5": {
        "date": "September 2022",
        "notes": [
            {
                "id": "a7c24b0740b633b8184e8d8e7c9acc7827866053",
                "type": "FIX",
                "text": " The out parameters in a value help on a action parameter dialog are now always considered",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "002075129400006130122022"
                    }
                ]
            },
            {
                "id": "c83a1059a7a613ff7b4c3501f2781d7b40ec8efb",
                "type": "FIX",
                "text": " The Table and FilterBar building blocks now work correctly in custom sections of an object page",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280140763"
                    },
                    {
                        "type": "BCP",
                        "reference": "2280148549"
                    }
                ]
            },
            {
                "id": "4c9d05d05bdbece23caa844d5edf4daa048c91af",
                "type": "FIX",
                "text": " We now trigger a new GET request whenever a user clicks the Go button in a filter bar of a value help dialog",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "002075129500004766202022"
                    }
                ]
            },
            {
                "id": "98008679f54a3c0ef5c5b42748e80215b3d3feb8",
                "type": "FIX",
                "text": " We now support sorting of lineItems that have contacts",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280172311"
                    }
                ]
            },
            {
                "id": "aa24512a3af8a5353ff5f6ba926ec2c95e707465",
                "type": "FIX",
                "text": " The pageReady event is now fired for an object page containing a filter bar, a table and a chart",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280155418"
                    }
                ]
            }
        ]
    }
}