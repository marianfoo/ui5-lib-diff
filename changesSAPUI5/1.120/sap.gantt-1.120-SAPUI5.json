{
    "1.120.0": {
        "date": "Oct 2023",
        "notes": [
            {
                "id": " Ibf92ee75a450c362ea60bfd902e60c9b4b2e3e3b",
                "text": "We've now extended pseudo shape support for the GanttChart shapes",
                "type": "FEATURE",
                "references": []
            }
        ]
    },
    "1.120.13": {
        "date": "Apr 2024",
        "notes": [
            {
                "id": "I388e52289a4844baf50a5aa11f01cf365a9d05cd",
                "text": "Avoid creating invalidate promise on table update when the update reason is 'UNKNOWN'",
                "type": "FIX",
                "references": []
            }
        ]
    },
    "1.120.16": {
        "date": "Jun 2024",
        "notes": [
            {
                "id": "Iff3a5e7c3e5802a5cb227e4c940f52aef8d6566e",
                "text": "We've now fixed an issue that caused unintended row bindings while interacting with draggable shapes in the chart",
                "type": "FIX",
                "references": [
                    {
                        "type": "SNOW",
                        "reference": "CS20240007818457"
                    }
                ]
            },
            {
                "id": "Ic58dd7f150f7f1397884db0a21d4095cb345ac65",
                "text": "We've now fixed the issues with the dynamic update of overlays in the expanded row scenario",
                "type": "FIX",
                "references": [
                    {
                        "type": "SNOW",
                        "reference": "DINC0119990"
                    }
                ]
            }
        ]
    },
    "1.120.18": {
        "date": "Jul 2024",
        "notes": [
            {
                "id": "I14293c7ae7dab66fd6ec1a445cdfda43bd75c632",
                "text": "We've now provided the option to enable or disable the appearance of multiple ghost shapes upon dragging in Gantt 2.0",
                "type": "FIX",
                "references": [
                    {
                        "type": "SNOW",
                        "reference": "CS20240007942243"
                    }
                ]
            }
        ]
    },
    "1.120.20": {
        "date": "Aug 2024",
        "notes": [
            {
                "id": "I70df64f956ddd4abc9e30b5e67dcfa237da2de10",
                "text": "We've now added a delay in scrolling if horizontal lazy loading is enabled for any chart",
                "type": "FIX",
                "references": [
                    {
                        "type": "SNOW",
                        "reference": "DINC0188115"
                    }
                ]
            },
            {
                "id": "I2c00d21a9c31c0fb65201a4418389c1c352718c4",
                "text": "We now suppress the call of the invalidate function on setShapeSelectionMode",
                "type": "FIX",
                "references": [
                    {
                        "type": "SNOW",
                        "reference": "CS20240008056667"
                    }
                ]
            }
        ]
    },
    "1.120.21": {
        "date": "Sep 2024",
        "notes": [
            {
                "id": "I1170030088710a6d9780238c7b3bc77b7fd73edb",
                "text": "We now retain the selection of a shape in Gantt chart irrespective of the row index",
                "type": "FIX",
                "references": [
                    {
                        "type": "SNOW",
                        "reference": "CS20240008222755"
                    }
                ]
            },
            {
                "id": "I3fcd035ac6413244306fa6d985cfaa5b83989201",
                "text": "We now ensure that the unused orphaned connector instances are destroyed correctly in cases where the related shapes are not visible",
                "type": "FIX",
                "references": [
                    {
                        "type": "SNOW",
                        "reference": "CS20240008126356"
                    }
                ]
            },
            {
                "id": "I724cb531bad20be5f5164c2dd0bd0a337aa6a0a4",
                "text": "We've now modified AggregationUtils method to avoid considering dragDropConfig as a shape aggregation of row settings",
                "type": "FIX",
                "references": [
                    {
                        "type": "SNOW",
                        "reference": "CS20240008091426"
                    }
                ]
            }
        ]
    },
    "1.120.23": {
        "date": "Nov 2024",
        "notes": [
            {
                "id": "Ic695857463ad2aa4a57279e585acbb41340e457d",
                "text": "We've now provided a fix for the auto proportionate zoom issue that occurred on initial rendering of the Gantt Chart",
                "type": "FIX",
                "references": [
                    {
                        "type": "SNOW",
                        "reference": "CS20240007023172"
                    }
                ]
            }
        ]
    },
    "1.120.26": {
        "date": "Feb 2025",
        "notes": [
            {
                "id": "I649776d77040f87d11167f14dee8ac23c1d6714b",
                "text": "We now ensure that the default time formatter is updated when the localization is changed",
                "type": "FIX",
                "references": [
                    {
                        "type": "SNOW",
                        "reference": "CS20240008704362"
                    }
                ]
            }
        ]
    },
    "1.120.32": {
        "date": "Jun 2025",
        "notes": [
            {
                "id": "I74df6228081872e935dda1f30301b57e2ed1081c",
                "text": "We've now added an aria-hidden attribute within Gantt chart to avoid the chart area from getting read out in the accessibility tool",
                "type": "FIX",
                "references": [
                    {
                        "type": "SNOW",
                        "reference": "CS20250010057150"
                    }
                ]
            }
        ]
    }
}