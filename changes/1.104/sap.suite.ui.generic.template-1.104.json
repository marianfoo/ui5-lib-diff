{
    "1.104.0": {
        "date": "June 2022",
        "notes": [
            {
                "id": "I427c4bb3bbd2f7b03a724eaaeca6ec2e114136c3",
                "type": "FEATURE",
                "text": "The loading indicator from side effect has been removed"
            },
            {
                "id": "I352f428a3241745f5fa008529f191d763b8f39d2",
                "type": "FEATURE",
                "text": "An option has been provided to define new keyboard shortcuts for DataFieldForAction and DataFieldForIntentBasedNavigation actions"
            },
            {
                "id": "I1cdc1597b2dc3231472939348db97b6d7c296688",
                "type": "FEATURE",
                "text": "Information density has been improved for the object page"
            },
            {
                "id": "I90f388140e07b9d4399879f97f599c0d91274011",
                "type": "FEATURE",
                "text": "Navigation restriction now takes priority over insert restrictions "
            },
            {
                "id": "Ifb626a29cf4f1a29fb405b94d4e7115ad5a5cb71",
                "type": "FEATURE",
                "text": "An option has been provided for specifying the allowed actions in the related apps functionality"
            },
            {
                "id": "Icf85e5600954ddb6c2b62fb1ea099354e8ed7423",
                "type": "FEATURE",
                "text": "The performance for DELETE & GET request for the object page and sub object page deletions has been improved"
            },
            {
                "id": "Ic24f02f96ffbc282cc12090d1fc20254985e535d",
                "type": "FEATURE",
                "text": "An extension point has provided Fiori Elements for applications to implement refresh as an entity, when ext.nav is triggered from reuse component"
            },
            {
                "id": "I4bd4529dd378ddb65b27b6e035ffaa7a703bac79",
                "type": "FEATURE",
                "text": "Reuse component handling after clicking Save has been improved"
            },
            {
                "id": "I448957aec7819b52d8cbc696acc526697613bfb1",
                "type": "FEATURE",
                "text": "Default values are now supported along with UX enhancements for empty rows creation"
            },
            {
                "id": "I0b3f36137fd19c49bdafb5a5bb54200f414e67af",
