{
    "1.106.0": {
        "date": "August 2022",
        "notes": [
            {
                "id": "3c79cc22c55996d1582387b44cc48e017c7e1ec3",
                "type": "FIX",
                "text": " We now remove transition messages only after a user clicks 'Delete' in the confirmation dialog",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280165552"
                    }
                ]
            },
            {
                "id": "1d4ad90dd27ecfbfe7416c91cee14230b322c2f5",
                "type": "FEATURE",
                "text": " We now provide the possibility to use a default action handler for menu buttons in list reports, object page headers, and forms"
            },
            {
                "id": "b0b0a4f0c4cea26328cf485342d5e69f872bd73a",
                "type": "FIX",
                "text": " We've updated the handling of a separator when processing the target entity",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280162139"
                    }
                ]
            },
            {
                "id": "fe4b802ed8cf1b05a8f7cd3d48f1e7baf2034a4c",
                "type": "FEATURE",
                "text": " We've added further features to the building blocks for tables and columns"
            },
            {
                "id": "181ee378e564a007686200609eec6b0aac4e0664",
                "type": "FIX",
                "text": " We've fixed the issue related to the setting of applicable contexts for the enablement of actions in a table",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280128435"
                    }
                ]
            },
            {
                "id": "567acb93a614d47f97ea5b7c55c2e65164e58ab6",
                "type": "FIX",
                "text": " The 'filter bar' building block is now generated properly if a metaPath is used that doesn't depend on the contextPath",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280140763"
                    }
                ]
            },
            {
                "id": "927ebf8fb6237606f7fc9e9585f1ceaeb4e59950",
                "type": "FIX",
                "text": " We now don't show a description for the header if you haven't defined one",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280149312"
                    }
                ]
            },
            {
                "id": "f8ded568cdc2cc7170f0080d1013f7e5efc75715",
                "type": "FIX",
                "text": "  SAP Fiori elements for OData V4 now supports the width calculation of columns that refer to navigation properties",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280157539"
                    }
                ]
            },
            {
                "id": "d9d6d5fcd938a8f95d0f07654ac1f6a0f0faaed8",
                "type": "FIX",
                "text": " The default value for a semantic date is now correctly set for the property of the navigation entity set",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280149845"
                    }
                ]
            },
            {
                "id": "f3daccfdb85b6fc7db10d10d29e2468c01a7e131",
                "type": "FIX",
                "text": " We have fixed the applyNavigationParameters controller extension",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280140596"
                    }
                ]
            },
            {
                "id": "ad307a37696f9b6e92158efbf556373a9d3cbdf3",
                "type": "FIX",
                "text": " We now ensure the app component doesn't break if you're not using an OData V4 service",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280144006"
                    }
                ]
            },
            {
                "id": "fb36a698bf60812caba9e90c25739ee7a8f9f54d",
                "type": "FIX",
                "text": " We now display the correct i18n text when a different entitySet is used in 'Delete' scenarios",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280115885"
                    }
                ]
            },
            {
                "id": "25597998e066ec3af2f1577a53cd53877324ed9b",
                "type": "FEATURE",
                "text": " We've enabled the handling of side effects for mass edit"
            },
            {
                "id": "159e4da54bc29d5941dfd630d600b88b14a9f039",
                "type": "FIX",
                "text": " The filter restrictions for the edm.Date property now work correctly",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280116999"
                    }
                ]
            },
            {
                "id": "5052e01c851190c4654ff5aa5d935ace6cacfe9a",
                "type": "FIX",
                "text": " SAP Fiori elements now refreshes the file name after the upload"
            },
            {
                "id": "5111476b046b29368a9874994196cd29243ba1de",
                "type": "FIX",
                "text": " We've fixed an issue relating to side effects in combination with columns added via table settings",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280137459"
                    }
                ]
            },
            {
                "id": "1383b3599239dbf4bf51d2bfc12ecd6eac8b5310",
                "type": "FEATURE",
                "text": " SAP Fiori elements for OData V4 has replaced the deprecated AggregatedProperties with AggregatedProperty"
            },
            {
                "id": "ae1f2dd46df61872e73a3f2bc730cbf961b0ae38",
                "type": "FEATURE",
                "text": " The EditFlow controller extension InvokeAction now returns a promise which provides the response of the action",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280131008"
                    }
                ]
            },
            {
                "id": "70599a5448512b5b1244d2c65327d1e26f87d5b0",
                "type": "FIX",
                "text": " We now correctly apply SemanticObjectMappings when navigating via a SemanticObject that is dynamic",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280145046"
                    }
                ]
            },
            {
                "id": "ed8a8174c1076f62212a157dad1409e18542c39e",
                "type": "FIX",
                "text": " UI.hidden annotation now works correctly for the header.info property",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280149123"
                    }
                ]
            },
            {
                "id": "55e2c4bd97381ffe310d50d6d77f305f3d65cbb0",
                "type": "FIX",
                "text": " We now remove the text arrangement property when #textOnly is used from the column's propertyInfo object",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280137173"
                    }
                ]
            },
            {
                "id": "f8b03a17f2cd366c43c9fe5adb48bfafc5824f80",
                "type": "FIX",
                "text": " Pressing the ESC key in a dialog now has the same result as clicking the Cancel button",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280133523"
                    }
                ]
            },
            {
                "id": "4866e04fee9ab957dfe01e830a91fb550dd6aed1",
                "type": "FEATURE",
                "text": " we now support for semantic date range default values that require operators with parameters"
            },
            {
                "id": "bb65668e1d0109fcb45a8e91d0f6bd2daeb145d1",
                "type": "FIX",
                "text": " The title of an object page linked to a ConnectedFields is now displayed properly",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280140875"
                    }
                ]
            },
            {
                "id": "42cf99ead92f67b457af71ab42f83420df6722b3",
                "type": "FIX",
                "text": " We now read default values of parameters of unbound actions"
            },
            {
                "id": "255613a79e43b14e4a3aacdd08d2751b809dbd2e",
                "type": "FIX",
                "text": " The TargetValue for the ProgressIndicator data point is now set correctly",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280125412"
                    }
                ]
            },
            {
                "id": "da326cbd140bb26c0ef705336dd633bb90c15862",
                "type": "FIX",
                "text": " We now show the correct title in an action parameter dialog when the action label is undefined",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280141181"
                    }
                ]
            },
            {
                "id": "351b1e248cc9d3a01fad6a793b1d29720c053b56",
                "type": "FIX",
                "text": " We now display the correct i18n text also in 'Delete' scenarios",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280115885"
                    }
                ]
            },
            {
                "id": "06ca667be1cc10d4873a3e0a7aa43ddb8bb8e4f8",
                "type": "FEATURE",
                "text": " Customers can now override the action name in the action parameter dialog"
            },
            {
                "id": "3f37c3c78635edba089359112e148c073ec57510",
                "type": "FIX",
                "text": " The filters from the default SelectionVariant annotation are no longer considered in a list report with multiple views"
            },
            {
                "id": "fbc9006b9637f6ce4adac40ffaa611929de03596",
                "type": "FIX",
                "text": " We now pass the correct component to the API of the CacheHandlerServiceFactory",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270021471"
                    }
                ]
            },
            {
                "id": "135258c567b4cc4ffd785a6b3036d45e4a1fb570",
                "type": "FIX",
                "text": " SAP Fiori elements now enables you to filter on a table for navigation properties in the LineItem",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2170295934"
                    }
                ]
            },
            {
                "id": "debef7af22ace7c19135a9d007aa2353bd7be84d",
                "type": "FEATURE",
                "text": " SAP Fiori elements for OData V4 now supports custom navigation using a 1:1 association"
            },
            {
                "id": "0937b4ae1da8749d91f53e8dbd1fe128eb07b7de",
                "type": "FIX",
                "text": " SAP Fiori elements now properly supports the default PresentationVariant for list reports using multiple table mode",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280096831"
                    }
                ]
            },
            {
                "id": "a1679bc6fae93ca628ac7934d88307b194dbb8c7",
                "type": "FIX",
                "text": " We've solved the issue relating to the message popover displaying non-applicable messages",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280113192"
                    }
                ]
            },
            {
                "id": "c381b7e0b2ab73bee841f84cb55a7a53235276cd",
                "type": "FIX",
                "text": " The confirmation popup now shows the correct severity of a message in alignment with the back end",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280122192"
                    }
                ]
            },
            {
                "id": "d7acbfa3f9d9a49d6a96ff32512eb84d404a1ec6",
                "type": "FIX",
                "text": " SAP Fiori elements for OData V4 now supports table visualization even if no lineItem annotations have been defined",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280114335"
                    }
                ]
            },
            {
                "id": "f210b944bed06634ed63307fe113993bd54417e3",
                "type": "FIX",
                "text": " The PREPARE action now requests the SAP_Messages if the ACTIVE action fails",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270084075"
                    }
                ]
            },
            {
                "id": "2e2e6ebe07f0cec98e0f747e08125f2de307ba9e",
                "type": "FEATURE",
                "text": " You can now mark custom filter fields as 'mandatory' by setting the 'required' property in the manifest.json"
            },
            {
                "id": "7dea0a8d56bffda7b84720bfb938cd9c3e56735c",
                "type": "FIX",
                "text": " If the creation of a new object fails in a table using the flexible column layout, the object is no longer shown in the table",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280117541"
                    }
                ]
            },
            {
                "id": "0dc2154badebfd25c4338cf65752340d50a954fd",
                "type": "FIX",
                "text": " You can now again activate drafts on RAP",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280125855"
                    }
                ]
            },
            {
                "id": "f7424430643f2ff2a167e6234e469c97e22bd305",
                "type": "FIX",
                "text": " The previous binding changes are being reset and destroyed before a new binding is created",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280041447"
                    }
                ]
            },
            {
                "id": "2a890b2756157b12b3a23d13b6d1e638dd4293de",
                "type": "FIX",
                "text": " The FilterBar building block now works correctly even if no filter facet target is found",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280120218"
                    }
                ]
            },
            {
                "id": "02c1fc26826036f7fa2e7ad6ef97c6399e5db8ca",
                "type": "FIX",
                "text": " The filter range set via a SelectionVariant annotation is now correctly applied",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280121234"
                    }
                ]
            },
            {
                "id": "b02663cfd9cd96a7e2c1814a7c53fbc3108f2ff2",
                "type": "FIX",
                "text": " We've deprecated storeInnerAppstate and replaced it with storeInnerAppStateAsync",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280116507"
                    }
                ]
            },
            {
                "id": "8e2f13c98c01db373d968dc90ed3dc32cc47ae01",
                "type": "FIX",
                "text": " Complex conditional expressions using `Not` are now working as expected",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280125081"
                    }
                ]
            },
            {
                "id": "447f60056411a6de21bc40daff3029f7eea7b443",
                "type": "FIX",
                "text": " The issue related to the calculation of the column width has been resolved",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280120627"
                    }
                ]
            },
            {
                "id": "2efebf41784642621119db29b17615ab94e347cd",
                "type": "FIX",
                "text": " SAP Fiori elements for OData V4 now shows a warning in the browser console, if an invalid position anchor was defined for an item in manifest.json",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280094327"
                    },
                    {
                        "type": "BCP",
                        "reference": "2270021210"
                    }
                ]
            },
            {
                "id": "6a99b5d32b98cb51de25ff670f0c196bbc67740b",
                "type": "FIX",
                "text": " A table in an object page using the flexible column layout is now updated when a new object is created",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280113762"
                    }
                ]
            },
            {
                "id": "2dcd250dbe00ea9cdb1ff3df5b485303dcc1f859",
                "type": "FIX",
                "text": " Side effects in fnOnStrictHandlingFailed have been requested",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280095030"
                    }
                ]
            },
            {
                "id": "6d0b1fee74d0e7d76fa35893a8e8ee6939d7084b",
                "type": "FIX",
                "text": " The action button has been renamed to align with V2",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280108081"
                    }
                ]
            }
        ]
    }
}