{
    "1.105.0": {
        "date": "July 2022",
        "notes": [
            {
                "id": "da326cbd140bb26c0ef705336dd633bb90c15862",
                "type": "FIX",
                "text": " We now show the correct title in an action parameter dialog when the action label is undefined",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280141181"
                    }
                ]
            },
            {
                "id": "351b1e248cc9d3a01fad6a793b1d29720c053b56",
                "type": "FIX",
                "text": " We now display the correct i18n text also in 'Delete' scenarios",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280115885"
                    }
                ]
            },
            {
                "id": "06ca667be1cc10d4873a3e0a7aa43ddb8bb8e4f8",
                "type": "FEATURE",
                "text": " Customers can now override the action name in the action parameter dialog"
            },
            {
                "id": "3f37c3c78635edba089359112e148c073ec57510",
                "type": "FIX",
                "text": " The filters from the default SelectionVariant annotation are no longer considered in a list report with multiple views"
            },
            {
                "id": "fbc9006b9637f6ce4adac40ffaa611929de03596",
                "type": "FIX",
                "text": " We now pass the correct component to the API of the CacheHandlerServiceFactory",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270021471"
                    }
                ]
            },
            {
                "id": "135258c567b4cc4ffd785a6b3036d45e4a1fb570",
                "type": "FIX",
                "text": " SAP Fiori elements now enables you to filter on a table for navigation properties in the LineItem",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2170295934"
                    }
                ]
            },
            {
                "id": "debef7af22ace7c19135a9d007aa2353bd7be84d",
                "type": "FEATURE",
                "text": " SAP Fiori elements for OData V4 now supports custom navigation using a 1:1 association"
            },
            {
                "id": "0937b4ae1da8749d91f53e8dbd1fe128eb07b7de",
                "type": "FIX",
                "text": " SAP Fiori elements now properly supports the default PresentationVariant for list reports using multiple table mode",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280096831"
                    }
                ]
            },
            {
                "id": "a1679bc6fae93ca628ac7934d88307b194dbb8c7",
                "type": "FIX",
                "text": " We've solved the issue relating to the message popover displaying non-applicable messages",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280113192"
                    }
                ]
            },
            {
                "id": "c381b7e0b2ab73bee841f84cb55a7a53235276cd",
                "type": "FIX",
                "text": " The confirmation popup now shows the correct severity of a message in alignment with the back end",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280122192"
                    }
                ]
            },
            {
                "id": "d7acbfa3f9d9a49d6a96ff32512eb84d404a1ec6",
                "type": "FIX",
                "text": " SAP Fiori elements for OData V4 now supports table visualization even if no lineItem annotations have been defined",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280114335"
                    }
                ]
            },
            {
                "id": "f210b944bed06634ed63307fe113993bd54417e3",
                "type": "FIX",
                "text": " The PREPARE action now requests the SAP_Messages if the ACTIVE action fails",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270084075"
                    }
                ]
            },
            {
                "id": "2e2e6ebe07f0cec98e0f747e08125f2de307ba9e",
                "type": "FEATURE",
                "text": " You can now mark custom filter fields as 'mandatory' by setting the 'required' property in the manifest.json"
            },
            {
                "id": "7dea0a8d56bffda7b84720bfb938cd9c3e56735c",
                "type": "FIX",
                "text": " If the creation of a new object fails in a table using the flexible column layout, the object is no longer shown in the table",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280117541"
                    }
                ]
            },
            {
                "id": "0dc2154badebfd25c4338cf65752340d50a954fd",
                "type": "FIX",
                "text": " You can now again activate drafts on RAP",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280125855"
                    }
                ]
            },
            {
                "id": "f7424430643f2ff2a167e6234e469c97e22bd305",
                "type": "FIX",
                "text": " The previous binding changes are being reset and destroyed before a new binding is created",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280041447"
                    }
                ]
            },
            {
                "id": "2a890b2756157b12b3a23d13b6d1e638dd4293de",
                "type": "FIX",
                "text": " The FilterBar building block now works correctly even if no filter facet target is found",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280120218"
                    }
                ]
            },
            {
                "id": "02c1fc26826036f7fa2e7ad6ef97c6399e5db8ca",
                "type": "FIX",
                "text": " The filter range set via a SelectionVariant annotation is now correctly applied",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280121234"
                    }
                ]
            },
            {
                "id": "b02663cfd9cd96a7e2c1814a7c53fbc3108f2ff2",
                "type": "FIX",
                "text": " We've deprecated storeInnerAppstate and replaced it with storeInnerAppStateAsync",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280116507"
                    }
                ]
            },
            {
                "id": "8e2f13c98c01db373d968dc90ed3dc32cc47ae01",
                "type": "FIX",
                "text": " Complex conditional expressions using `Not` are now working as expected",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280125081"
                    }
                ]
            },
            {
                "id": "447f60056411a6de21bc40daff3029f7eea7b443",
                "type": "FIX",
                "text": " The issue related to the calculation of the column width has been resolved",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280120627"
                    }
                ]
            },
            {
                "id": "2efebf41784642621119db29b17615ab94e347cd",
                "type": "FIX",
                "text": " SAP Fiori elements for OData V4 now shows a warning in the browser console, if an invalid position anchor was defined for an item in manifest.json",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280094327"
                    },
                    {
                        "type": "BCP",
                        "reference": "2270021210"
                    }
                ]
            },
            {
                "id": "6a99b5d32b98cb51de25ff670f0c196bbc67740b",
                "type": "FIX",
                "text": " A table in an object page using the flexible column layout is now updated when a new object is created",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280113762"
                    }
                ]
            },
            {
                "id": "2dcd250dbe00ea9cdb1ff3df5b485303dcc1f859",
                "type": "FIX",
                "text": " Side effects in fnOnStrictHandlingFailed have been requested",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280095030"
                    }
                ]
            },
            {
                "id": "6d0b1fee74d0e7d76fa35893a8e8ee6939d7084b",
                "type": "FIX",
                "text": " The action button has been renamed to align with V2",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280108081"
                    }
                ]
            },
            {
                "id": "ddca26d6395141d4ca7e3f112b7cd1ff9925424b",
                "type": "FIX",
                "text": " Editing numbers with units having a value help no longer shows invalid error message",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280117482"
                    }
                ]
            },
            {
                "id": "988880b1b9b2288f358ad0538c933edfb3cb7287",
                "type": "FIX",
                "text": "The correct title and icons for messages in message dialog are now displayed",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280109574"
                    }
                ]
            },
            {
                "id": "3062ab6b988e38305a755141c70b1a36d343e6ad",
                "type": "FIX",
                "text": " The issue where navigation was prevented from a custom section to a subobject page in an application using flexible column layout is now fixed",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280108256"
                    }
                ]
            },
            {
                "id": "30afb642f6b2e36a7e651d9c673af502a9c6267c",
                "type": "FIX",
                "text": " PresentationVariant definition are now correctly supported by MacroTable",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280109923"
                    }
                ]
            },
            {
                "id": "8ba4c8c8c4e8df68c4ccd9c261409c091be9b85b",
                "type": "FIX",
                "text": " Using a dynamic field control in SAP Fiori elements, currencies that are set to readonly are now correctly displayed",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280101885"
                    }
                ]
            },
            {
                "id": "4f2b685e188c958946dec4bebf74365285ccdca3",
                "type": "FIX",
                "text": " SAP messages are correctly retrieved by the prepareAction function, when a new draft is created",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270084075"
                    }
                ]
            },
            {
                "id": "cd25c639bf7cee2a47511a8528053f4ba68fcb40",
                "type": "FEATURE",
                "text": " The form building block now accepts custom form elements that are defined inline"
            },
            {
                "id": "22d404f4cdb4793ab0bda47648f622ef6ea32738",
                "type": "FIX",
                "text": " Semantic object mapping now works correctly in the QuickView links",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270011959"
                    }
                ]
            },
            {
                "id": "266d1f141b9a221ffea7d78dbbc791bbcfcbe800",
                "type": "FIX",
                "text": " The enablement for continue button in creation dialog now works correctly",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280094744"
                    }
                ]
            },
            {
                "id": "ec428e6f3721166633fdaf4c2751bfa59d026b08",
                "type": "FIX",
                "text": ": We now ensure that semantic date operators are set correctly for the semantic object navigation",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280098253"
                    }
                ]
            },
            {
                "id": "bdfd23f3b672616ad0347b9f9fa9791692d4564a",
                "type": "FIX",
                "text": " We now support the Chart annotation within the collection facet",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280101265"
                    }
                ]
            },
            {
                "id": "0e26ea3294d5542da264d7156eba181492ab2663",
                "type": "FIX",
                "text": " The width for a FieldGroup containing buttons is now correctly calculated",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280097226"
                    }
                ]
            },
            {
                "id": "3876caf1218e46ba2cdd93e8d5856916c81d8802",
                "type": "FEATURE",
                "text": " When an entity is draft enabled and has the annotation ApplySupported, we now use a responsive table as the fallback option"
            },
            {
                "id": "00b3faa73e2bb44db9f8899bb38977a4c0aae349",
                "type": "FIX",
                "text": " Semantic object mapping now works correctly in the QuickView Links",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270011959"
                    }
                ]
            },
            {
                "id": "e2191a1a93a713b7ca2364d4db3ba55244f71c2b",
                "type": "FIX",
                "text": " Multiple deletion is now possible for draft records",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270104900"
                    }
                ]
            },
            {
                "id": "5236e3b864b8bc0c6a6362f1b3d33d89ce510982",
                "type": "FEATURE",
                "text": " You can now use our customization settings to keep a draft version of your app when a forward navigation is triggered without displaying a confirmation popup"
            },
            {
                "id": "2286a93576e2e1917fdc1f5626525f246b58c638",
                "type": "FIX",
                "text": " You can now correctly edit currencies and units with a decimal property",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280096267"
                    }
                ]
            },
            {
                "id": "70104774347c318529589c8d1c34878a2d61d9b6",
                "type": "FIX",
                "text": " SAP Fiori elements now properly loads back-end messages in applications using flexible column layout",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280084728"
                    },
                    {
                        "type": "BCP",
                        "reference": "2270091338"
                    }
                ]
            },
            {
                "id": "20e8f20a63a3aa16230ddf675b0b235847bf9b07",
                "type": "FIX",
                "text": " The Continue button is now only activated once all mandatory fields are filled",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280087689"
                    }
                ]
            },
            {
                "id": "475d2137962894d74094ad6a4d0e1e078fa83596",
                "type": "FEATURE",
                "text": " We now automatically delete untouched drafts without a confirmation popup"
            },
            {
                "id": "79d8517d2b4ac82985d744d204624e26d1d2f586",
                "type": "FIX",
                "text": " We now filter and show only 412 messages in the strict handling dialog",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280095268"
                    }
                ]
            },
            {
                "id": "d3ab2d328a904578776a7b7b5ba4be8b8019543a",
                "type": "FIX",
                "text": " We've removed unnecessary queries when refreshing a list report that uses flexible column layout",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280086456"
                    }
                ]
            },
            {
                "id": "ff4ef2d02ce12ed852cc0c512d05aa65117f3419",
                "type": "FIX",
                "text": " We now clear messages from message dialog model after closing strict handling dialog",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270084923"
                    }
                ]
            },
            {
                "id": "4091eea19a31a8501078a4817fa6e027ee9e17bc",
                "type": "FIX",
                "text": " We're now using the correct message dialog box for navigation error messages",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270096270"
                    }
                ]
            },
            {
                "id": "3cf8d710b7ac6b91dab8583a6a553b993a2fbde7",
                "type": "FIX",
                "text": " Removing sensitive data now works correctly before showing the QuickView popover",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280085684"
                    }
                ]
            },
            {
                "id": "9390512c85dac85fa9c4cbedbf6dc09490f49166",
                "type": "FIX",
                "text": " We now trigger a side effect request on draft properties based on the requestProperty call",
                "references": [
                    {
                        "type": "BCP",
                        "reference": ""
                    }
                ]
            }
        ]
    },
    "1.105.1": {
        "date": "September 2022",
        "notes": [
            {
                "id": "f1657a13e001431939ba075ec5bbd21ddbc87fbb",
                "type": "FIX",
                "text": " The pageReady event is now fired for an object page containing a filter bar, a table and a chart",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270115472"
                    }
                ]
            },
            {
                "id": "529f31c9ea94a1ec75110c9853d75e06fde401cb",
                "type": "FIX",
                "text": " The issue related to the handling of transition message while updating a document is now fixed",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280158534"
                    }
                ]
            },
            {
                "id": "094eaf3a0d6b6e3f03679ce72d1836d08fbf8ed7",
                "type": "FIX",
                "text": " We now support sorting of lineItems that have contacts",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280172311"
                    }
                ]
            },
            {
                "id": "0f26dc4862db45389d7286d58707697dd00276a9",
                "type": "FIX",
                "text": " We've fixed an issue relating to side effects in combination with columns added via table settings",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280137459"
                    }
                ]
            }
        ]
    },
    "1.105.2": {
        "date": "October 2022",
        "notes": [
            {
                "id": "c315901e1e2d7b9b59a8029da0ab0bde2e34c86f",
                "type": "FIX",
                "text": " We now ensure the Table building blocks in a subobject page work correctly",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270128755"
                    }
                ]
            },
            {
                "id": "bd35c33b2264e62bfa119fd9cda406a4e35ba7d9",
                "type": "FIX",
                "text": " We now only show a description in the header of an object page if you've defined one",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280160899"
                    }
                ]
            },
            {
                "id": "2c8a6c066181da31037ff924301c1adf7c37d018",
                "type": "FIX",
                "text": " correction of documentation and types for SAP Fiori elements for OData v4 extensions",
                "references": [
                    {
                        "type": "BCP",
                        "reference": ""
                    }
                ]
            }
        ]
    },
    "1.105.3": {
        "date": "November 2022",
        "notes": [
            {
                "id": "b446a16f4b841b61afb8f9daa8da8fb9993aeada",
                "type": "FIX",
                "text": " We now ensure that the \"availability\" property of the line item annotation in a table is set correctly",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270180341"
                    }
                ]
            },
            {
                "id": "742f2aa48fbb07cffe6359a701cfe0d0d5725f4a",
                "type": "FIX",
                "text": " SAP Fiori elements now refreshes the file name after the upload",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270179757"
                    }
                ]
            }
        ]
    }
