{
    "1.120.0": {
        "date": "October 2023",
        "notes": [
            {
                "id": "46deab362613450c92722ec201adba7f54dad426",
                "type": "FIX",
                "text": " The message strip on the object page is now context-specific",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370116214"
                    }
                ]
            },
            {
                "id": "9a19d2f777aca065359470d1190f96df8fb2613c",
                "type": "FEATURE",
                "text": " We've now added a confirmation dialog that allows end users to invite other users in apps that support the collaborative draft mode"
            },
            {
                "id": "4417c03d42d4fa8035cc23323a584e4df6a7b04c",
                "type": "FEATURE",
                "text": " In apps that support the collaborative draft mode, we've now provided a confirmation dialog asking users to save or discard their changes when leaving a page"
            },
            {
                "id": "c84b4502dd116e6aaff9b6aca2c36e9a189fca6f",
                "type": "FIX",
                "text": " We now ensure that the header level for the object page table and macro chart are set correctly",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370094825"
                    }
                ]
            },
            {
                "id": "5918983fc72e7b05246579da49922c8c401a240e",
                "type": "FEATURE",
                "text": " We now display separate columns for the editing status and business situation in grid tables"
            },
            {
                "id": "08b458e578942f0353c3489f97c3bb5a73782319",
                "type": "FEATURE",
                "text": ": We've added a function using the extension API that selects a specific tab programmatically on the list report with multiple tabs"
            },
            {
                "id": "1efe2278a3c90fc34d2d6cf5bbe77d07df8f804e",
                "type": "FIX",
                "text": " We've resolved the issue of the Delete action not working when the header description is hidden",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370091706"
                    },
                    {
                        "type": "BCP",
                        "reference": "2370092980"
                    },
                    {
                        "type": "BCP",
                        "reference": "2370095588"
                    }
                ]
            },
            {
                "id": "f9cad4ac011c48e559e24b7470cbc8bd20bd19a1",
                "type": "FIX",
                "text": " We now show the empty indicator if a contact block link is not maintained",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370048588"
                    }
                ]
            },
            {
                "id": "dca0bff136a5cddb488184915b4f79569fa598e0",
                "type": "FIX",
                "text": " Filters with default values is now getting stored in selectionVariant and passed correctly during cross navigation",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370080159"
                    }
                ]
            },
            {
                "id": "3437b1702a16d09bc69394ae091a8c2d6d29f3b6",
                "type": "FIX",
                "text": " We now ensure that the secondary actions in the page-level toolbar appear in ghost style and the secondary actions in the content-level toolbar appear in transparent style.",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370067990"
                    }
                ]
            },
            {
                "id": "31406594569db584e5d57c8080fbbe1e2d0ca350",
                "type": "FIX",
                "text": " We now display the avatar in the correct aspect ratio within the collapsed header",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370099636"
                    }
                ]
            },
            {
                "id": "73c11e7fc94714860c13e110eec2a5f72b35208c",
                "type": "FEATURE",
                "text": " SAP Fiori elements for OData V4 now allows you to use the KPITag building block in list reports"
            },
            {
                "id": "c3e8bd8bb6b389867b80a4e40cc3a84013a7e901",
                "type": "FIX",
                "text": " We now ensure that accessing list report tables using the onTable() selector provided by the OPA API works correctly for actions",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370079138"
                    }
                ]
            },
            {
                "id": "a37cf42a59e703404e3ee9616f21f52b9afd521a",
                "type": "FEATURE",
                "text": " SAP Fiori elements for OData V4 now allows you to use the KPITag building block in list reports"
            },
            {
                "id": "9684125875e407ceb28b45604af2a6acd68dea75",
                "type": "FEATURE",
                "text": " When you click on the email field used in a table or a form, you can now use the communication options provided by Microsoft Teams"
            },
            {
                "id": "77271c571a2afd09f57b17a2006b444b9a9ad16f",
                "type": "FIX",
                "text": " We now evaluate the HeaderInfo.TypeNamePlural annotation as an empty string if it isn't defined",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2380101495"
                    }
                ]
            },
            {
                "id": "d97ce181ca90c56b69076aa997d42b48b355b010",
                "type": "FIX",
                "text": " We now ensure that the object page scrolls to the correct section when the user clicks on a data point",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2380109653"
                    }
                ]
            },
            {
                "id": "6e6fd324e5132a5c16d1a706e6e82466cb5c36b6",
                "type": "FEATURE",
                "text": " We now provide a setting for app developers to enable their users to automatically access the latest draft, if applicable, when creating a draft root entity"
            }
        ]
    },
    "1.120.1": {
        "date": "December 2023",
        "notes": [
            {
                "id": "e39b8d0a96b3d61ed89a6d66b84a9d02858d7da8",
                "type": "FEATURE",
                "text": " We now recalculate the default values in empty row mode if the table is refreshed using a side effect"
            },
            {
                "id": "6e15f8034acba8ec02340cb736f664629b93e113",
                "type": "FEATURE",
                "text": " We now support the preferredMode=edit parameter that launches an object page directly in edit mode",
                "references": [
                    {
                        "type": "BCP",
                        "reference": ""
                    }
                ]
            },
            {
                "id": "93110b32fcb484a4d586c6ee39b421249b170056",
                "type": "FIX",
                "text": " We now ensure that the relevant custom filters are also correctly applied to other entity sets",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370128365"
                    }
                ]
            },
            {
                "id": "239a03c3fa17dc0fada32d37f52056c61ff971b5",
                "type": "FIX",
                "text": " We now ensure that the opensOnFocus value of the MDC field is set based on the focusInfo parameter",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2380109591"
                    }
                ]
            },
            {
                "id": "46b50de2ce2a6b9aef8a597ab8aa0a0a2429869e",
                "type": "FIX",
                "text": " We've now added the prefix 'cmd:' to the commands defined in the manifest for actions",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370144348"
                    }
                ]
            },
            {
                "id": "f90f4d1a8d276a75cb238055b49f1d5385aeafd0",
                "type": "FIX",
                "text": " We now set the focus on the first editable field upon the initial load of the object page",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370145528"
                    }
                ]
            },
            {
                "id": "2c8486e82981ac422c4949996f08ad2fdc613a6f",
                "type": "FIX",
                "text": " We have resolved several issues when adding items using a MultiValueField",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370121682"
                    }
                ]
            },
            {
                "id": "7fbb5a0bf878fd79fc170cc227de26011e7edb02",
                "type": "FIX",
                "text": " We now ensure that the opensOnFocus value is set based on the focusInfo parameter",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2380109591"
                    },
                    {
                        "type": "BCP",
                        "reference": "2370126663"
                    }
                ]
            }
        ]
    },
    "1.120.6": {
        "date": "February 2024",
        "notes": [
            {
                "id": "fb17a38283b1bfe67e9933a64b4a3aef9b217987",
                "type": "FIX",
                "text": " We've resolved an issue with displaying the text annotation in the quick view title"
            },
            {
                "id": "1551439d389c4d8321ec5b908ddf09d1837a1a74",
                "type": "FIX",
                "text": " We have resolved an issue with exporting columns added using page variant management in a list report with multiple views"
            }
        ]
    },
    "1.120.8": {
        "date": "April 2024",
        "notes": [
            {
                "id": "69be7d3c77a4d4c29963323cbf54d66e3c547b55",
                "type": "FIX",
                "text": " If the manifest configuration for an object page uses a contextPath parameter instead of entitySet, we ensure that a confirmation dialog is displayed while navigating away from the object page in edit mode"
            }
        ]
    },
    "1.120.9": {
        "date": "April 2024",
        "notes": [
            {
                "id": "98b0d6d5003c99ebc98f4f10a239d9c0c60551b9",
                "type": "FEATURE",
                "text": " Applications can now dynamically display the ‘Summarize’ button if the prerequisites are fulfilled"
            }
        ]
    },
    "1.120.10": {
        "date": "May 2024",
        "notes": [
            {
                "id": "af239b51b36d42720a84663644fec0b7ea2f6764",
                "type": "FIX",
                "text": " We now ensure that the message popover is no longer shown in display mode for messages added using the table API addMessages"
            }
        ]
    },
    "1.120.11": {
        "date": "June 2024",
        "notes": [
            {
                "id": "183c39736fa35ff35c880766b7e9d7d9ebf112bc",
                "type": "FIX",
                "text": " We now display SAP Fiori launchpad when a user discards a draft by navigating back using the shell button if the object page is the first entry in the browser history"
            },
            {
                "id": "97f719fac6062cae0b455137ea173617de63b03c",
                "type": "FIX",
                "text": " The MessageButton now only displays validation errors for controls with validation errors and back-end errors"
            },
            {
                "id": "928b276122fd83eee68cfe497fdef5bd824876dc",
                "type": "FIX",
                "text": " We now ensure that the list report properties for multiple views annotated using UI.SelectionFields are available among other filter bar properties"
            }
        ]
    },
    "1.120.12": {
        "date": "July 2024",
        "notes": [
            {
                "id": "6d5adba94693599fccd980779f3255531f0e97cf",
                "type": "FIX",
                "text": " When applying the iAppState, we now wait until the object page is ready and then set the selected section"
            }
        ]
    },
    "1.120.13": {
        "date": "July 2024",
        "notes": [
            {
                "id": "f37fd3a1eaed5f41071acef2d63dd1e38bd76e98",
                "type": "FIX",
                "text": " We no longer show the subsections that are dynamically invisible on the anchor bar of the object page"
            },
            {
                "id": "5c67f9347112c54780b20d15fa9b20695df8f9cc",
                "type": "FIX",
                "text": " The MessageButton now only displays validation errors for controls outside of a table that have validation errors and back-end errors"
            }
        ]
    },
    "1.120.14": {
        "date": "July 2024",
        "notes": [
            {
                "id": "262d238d21c93d723f0a57064cbb25804516da27",
                "type": "FIX",
                "text": " The message strip of the list report is now recreated when the list report is reopened after going to the home page"
            }
        ]
    },
    "1.120.15": {
        "date": "August 2024",
        "notes": [
            {
                "id": "642a142efb0deae6dd3da8ae1d88307d31e7149f",
                "type": "FIX",
                "text": " We no longer request data for actions of hidden subsections"
            },
            {
                "id": "847252d461d0d270792be67bb5d4ba21381b95ef",
                "type": "FIX",
                "text": " We now correctly request the count of all visible tabs on the ListReport"
            }
        ]
    },
    "1.120.16": {
        "date": "September 2024",
        "notes": [
            {
                "id": "01fd49d19a2187b8205ea6cfb9473717851fa444",
                "type": "FIX",
                "text": "We now allow overflowing of object page header buttons in small screens"
            }
        ]
    },
    "1.120.17": {
        "date": "November 2024",
        "notes": [
            {
                "id": "3f5fd68c9f8e4053ae71b9819c8b63ea11d35269",
                "type": "FIX",
                "text": " You can now overwrite localization messages for Save action"
            },
            {
                "id": "81b237d9dcc482051700ec24b6a1459102f6f38f",
                "type": "FIX",
                "text": " We now display the correct label on the message button in the footer of the object page"
            },
            {
                "id": "c2fbabba7f5c948a81c6772a0ffd8acf117c9837",
                "type": "FIX",
                "text": " We now ensure that the title does not overlap with global actions on the object page"
            }
        ]
    },
    "1.120.18": {
        "date": "December 2024",
        "notes": [
            {
                "id": "3770fdfc1c9f2396ab54c3296e237b84eeee3927",
                "type": "FIX",
                "text": " We now ensure an iAppState is created and applied specifically to a view in flexible column layout apps"
            }
        ]
    },
    "1.120.20": {
        "date": "February 2025",
        "notes": [
            {
                "id": "5e9d8fa1786238b60946353709ff326685aeb63d",
                "type": "FIX",
                "text": " We now ensure that the URL parameter sap-ui-fe-variant-id works as expected"
            }
        ]
    },
    "1.120.24": {
        "date": "May 2025",
        "notes": [
            {
                "id": "8caf930344711905612b5841558e54575a293da6",
                "type": "FIX",
                "text": " We've resolved an issue with the standard variant management where data did not load initially when predefined filter fields were set and Apply Automatically was selected"
            }
        ]
    }
}