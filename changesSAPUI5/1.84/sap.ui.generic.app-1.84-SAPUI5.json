{
    "1.84.0": {
        "date": "November 2020",
        "notes": [
            {
                "id": "If8fc11cf8fe857b93634748b6c36fab6f6ccbf17",
                "type": "FEATURE",
                "text": "We are now showing a meaningful message in case of e-tag mismatch"
            },
            {
                "id": "If8630c6bef6e5ef0d35f89ad3221dc7e942f3697",
                "type": "FEATURE",
                "text": "We've now introduced the new OData syntax <String> for TargetProperties for the side effect annotations"
            },
            {
                "id": "I44a17585a1d909dc3fe1c3f8dbbd8a33a22f1df3",
                "type": "FEATURE",
                "text": "We now club the edit request and the header data request in one batch to avoid an additional sequential batch"
            },
            {
                "id": "I835cbd8be226a4c363501738840290ef75814fb3",
                "type": "FEATURE",
                "text": "We've now removed the code logic related to the SideEffectTypes as it is deprecated"
            }
        ]
    },
    "1.84.31": {
        "date": "December 2022",
        "notes": [
            {
                "id": "Ie53a2e981998a630defc9ba529d32811e21a1a5d",
                "type": "FIX",
                "text": "We've now fixed the issue of same E-Tag that was occurring in case of multiselct function import",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2070444890"
                    }
                ]
            },
            {
                "id": "I251678e86ba06147f22bd88b142bca056ad8b810",
                "type": "FIX",
                "text": "We now provide support for Preparation Action during side effect execution",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2070489214"
                    }
                ]
            },
            {
                "id": "I9d8e8c13997235385a12c4837df5514ab9f5fb94",
                "type": "FIX",
                "text": "We are now supporting the side effects that are annotated without effect type",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2080435998"
                    }
                ]
            }
        ]
    },
    "1.84.46": {
        "date": "June 2024",
        "notes": [
            {
                "id": "Ia5fb120c43f85d33614c8d55e113449e2f810d3c",
                "type": "FIX",
                "text": "We've now eliminated the ETag validation for draft apps that have the ETag enabled",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2180094228"
                    }
                ]
            },
            {
                "id": "I66a7ba41c403a6e12954faabbe75c0f109008699",
                "type": "FIX",
                "text": "Now we ensure that two side-effects are not executed when they are independent of each other and one of them has validation errors",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2070472962"
                    }
                ]
            },
            {
                "id": "Icb760d06be4103f554f7bb4bcb772236383e108a",
                "type": "FIX",
                "text": "The error messages are now processed only if the call back is present",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280043647"
                    }
                ]
            }
        ]
    }
}