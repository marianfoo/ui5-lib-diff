{
    "1.116.0": {
        "date": "July 2023",
        "notes": [
            {
                "id": "48ba6822427fd76f1b23757b7ee82be3e515a611",
                "type": "FIX",
                "text": " We've resolved an infinite recursion when processing a DataFieldForAnnotation pointing to a DataPoint with a dynamic target value",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2380062863"
                    }
                ]
            },
            {
                "id": "58ed5f567c96313249d653b15899134e73948d74",
                "type": "FEATURE",
                "text": " We now support onAfterXXX controller extensions on standard actions"
            },
            {
                "id": "c4c8a2d3dbdf5fd31b272d6f95849dc4eb6fc917",
                "type": "FEATURE",
                "text": " Non-nullable number fields now have 0 as the default value when the value of the field is deleted"
            },
            {
                "id": "9f149d9a44afcb28130f11f94d53d36a304c43ee",
                "type": "FIX",
                "text": " We now correctly handle the mandatory action parameters in a Create dialog that are set by Out parameters in the value help",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2380065096"
                    }
                ]
            },
            {
                "id": "c7f0cf9a341d069c089db271a5dbdff54affdf5b",
                "type": "FIX",
                "text": " We now prevent the screen reader from reading the dialog header 'Discard all Changes' twice",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2380053120"
                    }
                ]
            },
            {
                "id": "24f4babfa856b533208184921bd6f7e3be8eae2d",
                "type": "FIX",
                "text": " We have resolved an issue with navigating back from the object page when editing a subobject page",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2380051577"
                    }
                ]
            },
            {
                "id": "09b2c15039f561b37ef763b6671d82a338a88840",
                "type": "FIX",
                "text": " We now ensure that the Add Card to Insights option is hidden in by default when a user shares using Microsoft Teams",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2380054998"
                    }
                ]
            },
            {
                "id": "e10b91fc645a9354043eb4f7a9cbc92442cc4b1d",
                "type": "FIX",
                "text": " We have resolved an issue with the contextPath when using the loadFragment extension method",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2380073756"
                    }
                ]
            },
            {
                "id": "0942955ed1df75bca6c0a360c9d0d4809aa20b91",
                "type": "FIX",
                "text": " We now ensure that the option to retrigger the changeSet configured action with 412 message isn't provided if the user selects multiple contexts",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2380069495"
                    }
                ]
            },
            {
                "id": "966cd85659679216aedc9f7b0cd0eda84e444dfa",
                "type": "FEATURE",
                "text": " We now provide a byID method in the ExtensionAPI that allows accessing building blocks and custom controls"
            },
            {
                "id": "2c8d9c97599739419768747947966e03fb361523",
                "type": "FIX",
                "text": " We now set the property labels for contacts on the table's personalization dialog correctly",
