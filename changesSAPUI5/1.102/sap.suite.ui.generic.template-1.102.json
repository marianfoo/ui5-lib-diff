        "notes": [
            {
                "id": "Id819632a1e40ab7dae725eb902fb8e7861eb4c73",
                "type": "FEATURE",
                "text": "Date range control can now be rendered for date type parameter of a parameterized entity set"
            },
            {
                "id": "I4c82e479a423eddfee7de8e0a69e0aa367a9b797",
                "type": "FEATURE",
                "text": "Draft handling has been now been improved"
