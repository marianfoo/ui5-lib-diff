            {
                "id": "Ifc5ca961e0875c73328a0b94225be969927c4350",
                "type": "FIX",
                "text": "We now ensure that the LockedBy username is displayed in the list report object page when a full name is not present",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "002075129500009574622023"
                    }
                ]
            },
            {
