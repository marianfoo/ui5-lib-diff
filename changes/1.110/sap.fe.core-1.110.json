            {
                "id": "7984c5e87296fb120a3213da5c6043dbbb027c2e",
                "type": "FIX",
                "text": " We now support filtering on tables that have multi-level navigation properties",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280175163"
                    }
                ]
            },
            {
                "id": "4ee3c1e7aa3bd7daed7057edd5093fdbefb83973",
                "type": "FIX",
                "text": " We've resolved an issue related to static actions with the special 'ResultIsActiveEntity' parameter",
