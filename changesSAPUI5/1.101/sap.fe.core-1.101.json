{
    "1.101.0": {
        "date": "March 2022",
        "notes": [
            {
                "id": "936bfa4fa4b26da970393519bf569ffca32acc97",
                "type": "FIX",
                "text": " You can now use the Capabilities.NavigationRestrictions on NavigationProperties with containsTarget set to true (containments)",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280076065"
                    }
                ]
            },
            {
                "id": "91002ec15e4559d3e60221ab20996bb60cd265b1",
                "type": "FEATURE",
                "text": " V4:FE In SAP Fiori elements for OData V4, you can now use the new dynamic date range control for DateTime fields in the filter bar"
            },
            {
                "id": "51d0928a20611d147f0a4e084541ed48a6d91426",
                "type": "FEATURE",
                "text": " SAP Fiori elements for OData V4 now supports charts based on draft-enabled entity sets"
            },
            {
                "id": "e997f87ca30f63e0a2941746ec6356944ad37486",
                "type": "FEATURE",
                "text": " You can now define the visibility of a view when you're using multiple table mode in a list report"
            },
            {
                "id": "49d20e18e7c39ca129bf35136b5f9fcb56d80156",
                "type": "FIX",
                "text": " A filter field with a default value in the filter bar is now only shown if it is part of the selection fields",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280076159"
                    }
                ]
            },
            {
                "id": "6d46eff7dd3df56dc6138df9a4e7721ce00e3b1b",
                "type": "FIX",
                "text": " Draft handling is now processed correctly using flexible column layout when navigating to another object in the list report",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270039446"
                    }
                ]
            },
            {
                "id": "6c50242e61584119adf52c4b6a99b9fef504965a",
                "type": "FEATURE",
                "text": " The filter bar of the value help dialog is now expanded by default under certain conditions"
            },
            {
                "id": "9078f876d650381c3e8915e1177a409557f560d1",
                "type": "FIX",
                "text": " The visibility of the footer bar is now calculated correctly",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270015822"
                    }
                ]
            },
            {
                "id": "40e79cac05cb2a6a9649c1cd591a390bc8fb0575",
                "type": "FEATURE",
                "text": " In an FCL application the 3rd column isn't closed anymore when editing, saving or canceling"
            },
            {
                "id": "87b8527af40bc534dc0233b8502ea24823752b79",
                "type": "FIX",
                "text": " Once an OP table action or all actions within sub OP are performed, refresh the action context",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "002075129500001725022022"
                    }
                ]
            },
            {
                "id": "fdec946e3b9c05f6e3a3dee3620260f753857752",
                "type": "FIX",
                "text": " SAP Fiori elements for OData V4 now provides filtering on tables on the object page by default"
            },
            {
                "id": "96252644d9c96ee57323888ff828ede04268a7db",
                "type": "FEATURE",
                "text": " We've enhanced the 'Table' building block to support the isSearchable property"
            },
            {
                "id": "1db50713eb6f1d6c0044879d342e26674f006a40",
                "type": "FIX",
                "text": " The FieldControl annotations within ConnectedFields are now taken into account to indicate mandatory fields",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270013538"
                    }
                ]
            },
            {
                "id": "5711299b72a5cc9eefab6d9cd3ef4ab78845b4d3",
                "type": "FIX",
                "text": " You can now dynamically set the visibility of a custom subsection in the manifest",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270007410"
                    }
                ]
            },
            {
                "id": "67a069a30fbf477eaead6414b54419eed5d67c48",
                "type": "FEATURE",
                "text": " SAP Fiori elements now supports the property 'Importance' for building block columns"
            },
            {
                "id": "86995e970c96bcceaaf617cda6c850eba17ff042",
                "type": "FEATURE",
                "text": " The UI.Importance of a newly added column is now properly evaluated"
            },
            {
                "id": "b148d93d72f42fb0e8b12eb6f725a26c802211d9",
                "type": "FIX",
                "text": " The 'Continue' button in the 'Create' dialog is now disabled in case there are UI validation errors",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280043747"
                    }
                ]
            },
            {
                "id": "8be914258482a8e0c4202f884e7b1686fda7021b",
                "type": "FIX",
                "text": " The DraftIndicator status is now displayed as the last element in a FieldGroup"
            },
            {
                "id": "ff7fc38792739331743ee2535c860a1e4d47756b",
                "type": "FEATURE",
                "text": " We've enhanced the 'Action' building block: You can now group actions in a dropdown menu"
            },
            {
                "id": "c917cf7a8652dc6cb114a8a03b82f06fb376da3b",
                "type": "FIX",
                "text": " The warning text on an object page for the deletion of an object is now correctly displayed even if no description has been defined",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280065018"
                    }
                ]
            },
            {
                "id": "3ec63b86d53030ac56d2a1dd023b05004ad06596",
                "type": "FEATURE",
                "text": " You can now use singletons to implement restrictions on tables on the object page"
            },
            {
                "id": "79bdb3b369f79c50e0951b7bd93f23dc4060e7f8",
                "type": "FEATURE",
                "text": " SAP Fiori elements now supports the annotations DataFieldWithIntentBasedNavigation and DataFieldWithAction"
            },
            {
                "id": "8a62c4ecbb4719fcd1402e8ee950e778e3607211",
                "type": "FEATURE",
                "text": " You can now hide action parameters by using a dynamic expression"
            },
            {
                "id": "7462cab96c5f4f430f09ef40f4b473cdc2fcca78",
                "type": "FIX",
                "text": " The setFilterValues function now works correctly with semantic date operators",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280064776"
                    }
                ]
            },
            {
                "id": "ea9aceeb9afa5190af6b543523003624e2e848de",
                "type": "FEATURE",
                "text": " SAP Fiori elements now displays a confirmation popup for actions that trigger a warning with response code 412"
            },
            {
                "id": "909cd2d8bdc08fed12e3a4362c3c474adf88262b",
                "type": "FIX",
                "text": " The 'Invalid Fields Array' is now cleared when messages are empty",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280039473"
                    }
                ]
            },
            {
                "id": "07c6125768a405365f9863f58d465b5a498db9c1",
                "type": "FIX",
                "text": " SAP Fiori elements now displays a message page if the metadata isn't loaded correctly",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280072106"
                    }
                ]
            },
            {
                "id": "8bb5d9dcaf262ee547950411870252908a6e60e3",
                "type": "FIX",
                "text": " The persistent cache provided by SAP Fiori elements on a custom page now works correctly",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280074941"
                    }
                ]
            },
            {
                "id": "c7da7d0c98e20d4586e04164f237e75055f59387",
                "type": "FIX",
                "text": " Amounts with a corresponding currency are now correctly exported to a spreadsheet",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280037825"
                    }
                ]
            },
            {
                "id": "5625388611570d0cb36bd73b7cd64ed097163571",
                "type": "FEATURE",
                "text": " We've enhanced the 'Action' building block to support the requiresSelection property"
            },
            {
                "id": "487800b846fe08c26a97c176dee0e9f54470dbd6",
                "type": "FEATURE",
                "text": " Switching to edit mode now checks if there's a side effect with a trigger action"
            },
            {
                "id": "a2eb1cd525abf88c892d8e1889f82137714b0b18",
                "type": "FIX",
                "text": " We've restored the correct ordering of the fields in the filter bar",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280074987"
                    }
                ]
            },
            {
                "id": "29feca7464ce9d7e52021675a7317945e50e7d7e",
                "type": "FIX",
                "text": " The input value is now properly formatted when there is a unit or a currency",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280071897"
                    }
                ]
            },
            {
                "id": "92dd9d5feea8a32176ac472eaa6e50be8915e9bc",
                "type": "FEATURE",
                "text": " SAP Fiori elements now supports the 'Importance' property for custom columns"
            },
            {
                "id": "ce86dbc28b5af7cc6e50f40703cfa48fa97b0a9e",
                "type": "FEATURE",
                "text": " SAP Fiori elements has now added an API to send and remove messages in the 'Table' and 'Field' building blocks"
            },
            {
                "id": "d86974edb4c8fc9607dd3d0f436c9bf0a48fa72f",
                "type": "FIX",
                "text": " View caching now works properly even if no ETag is provided",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280064615"
                    }
                ]
            },
            {
                "id": "07ab5db589d1393869945e616821a633b009cc71",
                "type": "FIX",
                "text": " SAP Fiori elements now correctly displays the draft indicator for semantic key fields after adapting the column width",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2180405180"
                    }
                ]
            },
            {
                "id": "83a47f0df20d4019167ca1852d10232f4416a39a",
                "type": "FIX",
                "text": " Adding and removing custom filters in the flexible programming model now works correctly",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280052087"
                    }
                ]
            },
            {
                "id": "1ce9886cfb6fc96ec2229adeb54047607859ba73",
                "type": "FIX",
                "text": " The Create dialog for immutable fields now works correctly in case of errors",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280047255"
                    }
                ]
            },
            {
                "id": "c6d24ebe0d230f7cc5992a41d7b62315afc7f86f",
                "type": "FEATURE",
                "text": " Providing the same operators as V2 for new Dynamic Date Range Control"
            },
            {
                "id": "e5a9e8e117f8e8d0bbe339cfb2971502edd7bd13",
                "type": "FEATURE",
                "text": " The PropertyName within a SelectionOptions property of the SelectionVariant UI annotation can now be defined with a complex property"
            }
        ]
    }
}