{
    "1.96.0": {
        "date": "October 2021",
        "notes": [
            {
                "id": "27aa1acabae21399dd7f4906c7a3e68422f418b1",
                "type": "FEATURE",
                "text": " Usage of the new enableAutoColumnWidth property from MDC"
            },
            {
                "id": "f0f5c6e9a5656bcbb3a82d9c4bba716fba783d75",
                "type": "FEATURE",
                "text": " Toggling between draft and saved values is now also supported in the list report"
            },
            {
                "id": "bb8282a0894c1540494d42572d2f4c9844b0ef71",
                "type": "FIX",
                "text": " The MultiValueField no longer triggers any incorrect requests",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2170248261"
                    }
                ]
            },
            {
                "id": "0d30f0e03622065fc656670e199762ace538df36",
                "type": "FIX",
                "text": " The binding for the display of the unit of measure (UoM) has been changed"
            },
            {
                "id": "d504772e9ad1c3557cfb0ee044eda92bad5ecfdc",
                "type": "FIX",
                "text": " The issue regarding micro charts that don't have a unit of measure label has been fixed",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2170230064"
                    }
                ]
            },
            {
                "id": "6a8be7e6812ff2bbbfa8f2daa3a03f724b803cd2",
                "type": "FIX",
                "text": ": The entity of the table is now refreshed before the mass edit dialog is closed",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2170212651"
                    },
                    {
                        "type": "BCP",
                        "reference": "2170180909"
                    }
                ]
            },
            {
                "id": "d1c77e09ec9b8d39e3981dfa996fd62341b4396c",
                "type": "FEATURE",
                "text": " Boolean field are now dislayed as checkboxes in Edit mode, even if it has a ValueList annotation"
            },
            {
                "id": "15d68c5a9a7f3c638a5c983a31c3e730a69c7895",
                "type": "FIX",
                "text": " The 'Delete' button in the table toolbar now works correctly in custom sections",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2170218970"
                    }
                ]
            },
            {
                "id": "dfc0fa5aea1dad7f1e0bf5da5fa8528572fe6284",
                "type": "FIX",
                "text": " The runtime error that occurred when using the Table building block (macros:Table) in a custom page has been corrected",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2170218535"
                    }
                ]
            },
            {
                "id": "420009ee4272db3075ff4c80d2dc62b4056fbde5",
                "type": "FEATURE",
                "text": " The Paginator building block is now available"
            },
            {
                "id": "a3d9e55c20a9c6b6c9d684f7259b50cdf9876bb8",
                "type": "FEATURE",
                "text": " The FilterBar building block is now available"
            },
            {
                "id": "7f7f89226e40040ba406d2f35fb7857c7bded9d9",
                "type": "FIX",
                "text": " The keep-alive mode now also works for the navigation using a quick view title",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2170221014"
                    }
                ]
            },
            {
                "id": "a53cb88e985e27c1bb23e1a166d66f55e3540702",
                "type": "FIX",
                "text": " The quick view popup no longer calls the ushell hrefForExternal function for each link shown in the list",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2170217449"
                    }
                ]
            },
            {
                "id": "2679783bab5697092fd9b740ab7858813d91436f",
                "type": "FIX",
                "text": " Setting selectionMode to 'None' in the manifest now ensures that you cannot select table rows in Display mode",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2180311866"
                    }
                ]
            },
            {
                "id": "9eb2858802d0ae7ce8549e46bc20124b68568876",
                "type": "FIX",
                "text": " SAP Fiori elements now ensures the correct value help ID is used to check if value help exists",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2170173774"
                    },
                    {
                        "type": "BCP",
                        "reference": "2170184189"
                    }
                ]
            },
            {
                "id": "12106b4164aa5f44a03e2636974cc480e85677e2",
                "type": "FIX",
                "text": " The default sorter has been removed from the type-ahead of the field",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2170177929"
                    }
                ]
            },
            {
                "id": "9f2329cc6b5f84e7a185f9e3a78ca9c7b17c57b0",
                "type": "FEATURE",
                "text": " The Chart building block is now available"
            }
        ]
    },
