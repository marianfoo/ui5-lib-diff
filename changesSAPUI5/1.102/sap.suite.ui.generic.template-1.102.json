            {
                "id": "Ieef60315b96540899e1b0777911348fb963a5765",
                "type": "FEATURE",
                "text": "Keyboard shortcuts have been enabled for the custom action button"
            },
            {
                "id": "Ief508b2705f23957b706a5824ea8297d49dace99",
                "type": "FIX",
                "text": "FilterExpressionRestriction for semantic date picker has been included",
                "references": [
                    {
