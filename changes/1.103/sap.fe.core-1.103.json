                    {
                        "type": "BCP",
                        "reference": "2270046846"
                    }
                ]
            },
            {
                "id": "3bc4049d9818ed3d9b636e746892d9ac7859e6f9",
                "type": "FIX",
                "text": " When you click the OK button in the 'Session Expired' dialog you now correctly return to the previous page",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270072956"
                    }
                ]
            },
            {
                "id": "83248108f1d49b985491e8f4b391b3cf383140d6",
