    "1.102.5": {
        "date": "September 2022",
        "notes": [
            {
                "id": "35ca353752bfb3eb16c99010ad5cd4b3dda2e752",
                "type": "FIX",
                "text": " correction of documentation and types for SAP Fiori elements for OData v4 extensions",
                "references": [
                    {
                        "type": "BCP",
                        "reference": ""
                    }
                ]
            },
            {
                "id": "c83a1059a7a613ff7b4c3501f2781d7b40ec8efb",
                "type": "FIX",
                "text": " The Table and FilterBar building blocks now work correctly in custom sections of an object page",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280140763"
                    },
                    {
                        "type": "BCP",
                        "reference": "2280148549"
                    }
                ]
            },
            {
                "id": "98008679f54a3c0ef5c5b42748e80215b3d3feb8",
                "type": "FIX",
                "text": " We now support sorting of lineItems that have contacts",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280172311"
                    }
                ]
            },
            {
                "id": "aa24512a3af8a5353ff5f6ba926ec2c95e707465",
                "type": "FIX",
                "text": " The pageReady event is now fired for an object page containing a filter bar, a table and a chart",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280155418"
                    }
                ]
            }
        ]
    },
    "1.102.6": {
        "date": "October 2022",
