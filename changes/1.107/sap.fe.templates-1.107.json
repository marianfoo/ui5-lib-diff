{
    "1.107.0": {
        "date": "September 2022",
        "notes": [
            {
                "id": "bc50240cc5269c6dc6f228050e6cc94a7c829c93",
                "type": "FEATURE",
                "text": " We now provide the possibility to configure a handler function to control the visibility of actions"
            },
            {
                "id": "f3f7599e16c36b61a7cc213b2f62e3a18d18c022",
                "type": "FIX",
                "text": " The semantic keys now is showing correct text arrangement",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280160172"
                    }
                ]
            },
            {
                "id": "1365dbf59b6c78aa8f51e03be41df5aac2029b90",
                "type": "FIX",
                "text": " The object page now loads correctly if the section is not selected",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270117912"
                    },
                    {
                        "type": "BCP",
                        "reference": "2270116034"
                    }
                ]
            },
            {
                "id": "29442d1d91e5f4de8496f064fa620fbafbecec13",
                "type": "FIX",
                "text": " The context for navigation entity sets is now set correctly when doing outbound navigation",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280103290"
                    }
                ]
            },
            {
                "id": "b5e7b0ecda49875551b9bfac5777e107fbeea2e8",
                "type": "FIX",
                "text": " HeaderInfo TypeName now supports i18n text in the create mode",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280172084"
                    }
                ]
            },
            {
                "id": "b6e8689ffee51075fd63bb2fc82bba8ebac09da1",
                "type": "FIX",
                "text": " The QuickView now correctly displays the QuickViewFacets when there is only one direct application",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270112589"
                    }
                ]
            },
            {
                "id": "f35cdc4c7b2f6047a6f0642a9a4b900f223b2da3",
                "type": "FIX",
                "text": " We no longer display a clickable link in the message dialog if the error occurs in a hidden column of a table in an object page for which no navigation is configured",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280168899"
                    }
                ]
            },
            {
                "id": "2fc71f4d20cf4d06e96830d460a06bf4e3eb26fe",
                "type": "FIX",
                "text": " We no longer apply the viewState if the appState data isn't available",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280166356"
                    }
                ]
            },
            {
                "id": "d4266501aca938dc0ae6dea10eb6ad5d607c50c7",
                "type": "FIX",
                "text": " Navigation via a breadcrumb link now also works when the dimension names contain special characters like a forward slash ('/')",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280168478"
                    }
                ]
            },
            {
                "id": "f748d3a4a0fb0e739006eebaf8be8f47da06f835",
                "type": "FIX",
                "text": " The Table and FilterBar building blocks now work correctly in custom sections of an object page",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280140763"
                    },
                    {
                        "type": "BCP",
                        "reference": "2280148549"
                    }
                ]
            },
            {
                "id": "badd74da40407ca76e642238012cbd7adae48d9e",
                "type": "FIX",
                "text": " A quick view now shows the correct data after displaying another quick view before",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280160441"
                    }
                ]
            },
            {
                "id": "1d4ad90dd27ecfbfe7416c91cee14230b322c2f5",
                "type": "FEATURE",
                "text": " We now provide the possibility to use a default action handler for menu buttons in list reports, object page headers, and forms"
            },
            {
                "id": "b0b0a4f0c4cea26328cf485342d5e69f872bd73a",
                "type": "FIX",
                "text": " We've updated the handling of a separator when processing the target entity",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280162139"
                    }
                ]
            },
            {
                "id": "fe4b802ed8cf1b05a8f7cd3d48f1e7baf2034a4c",
                "type": "FEATURE",
                "text": " We've added further features to the building blocks for tables and columns"
            },
            {
                "id": "567acb93a614d47f97ea5b7c55c2e65164e58ab6",
                "type": "FIX",
                "text": " The 'filter bar' building block is now generated properly if a metaPath is used that doesn't depend on the contextPath",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280140763"
                    }
                ]
            },
            {
                "id": "927ebf8fb6237606f7fc9e9585f1ceaeb4e59950",
                "type": "FIX",
                "text": " We now don't show a description for the header if you haven't defined one",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280149312"
                    }
                ]
            },
            {
                "id": "d9d6d5fcd938a8f95d0f07654ac1f6a0f0faaed8",
                "type": "FIX",
                "text": " The default value for a semantic date is now correctly set for the property of the navigation entity set",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280149845"
                    }
                ]
            },
            {
                "id": "f3daccfdb85b6fc7db10d10d29e2468c01a7e131",
                "type": "FIX",
                "text": " We have fixed the applyNavigationParameters controller extension",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280140596"
                    }
                ]
            },
            {
                "id": "25597998e066ec3af2f1577a53cd53877324ed9b",
                "type": "FEATURE",
                "text": " We've enabled the handling of side effects for mass edit"
            },
            {
                "id": "33372b0182a4185321ae8639cfd6dee44b611d31",
                "type": "FIX",
                "text": " We've fixed the visibility behavior of the HeaderInfo if the header is editable",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280142010"
                    }
                ]
            },
            {
                "id": "36664b91787bfb00091e7e0c1427cab147c3e954",
                "type": "FIX",
                "text": " The UI.Hidden annotation now works correctly with reference facets pointing to another entity set via a navigation path",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280144773"
                    }
                ]
            },
            {
                "id": "1383b3599239dbf4bf51d2bfc12ecd6eac8b5310",
                "type": "FEATURE",
                "text": " SAP Fiori elements for OData V4 has replaced the deprecated AggregatedProperties with AggregatedProperty"
            },
            {
                "id": "ed8a8174c1076f62212a157dad1409e18542c39e",
                "type": "FIX",
                "text": " UI.hidden annotation now works correctly for the header.info property",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280149123"
                    }
                ]
            },
            {
                "id": "55e2c4bd97381ffe310d50d6d77f305f3d65cbb0",
                "type": "FIX",
                "text": " We now remove the text arrangement property when #textOnly is used from the column's propertyInfo object",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280137173"
                    }
                ]
            },
            {
                "id": "f8b03a17f2cd366c43c9fe5adb48bfafc5824f80",
                "type": "FIX",
                "text": " Pressing the ESC key in a dialog now has the same result as clicking the Cancel button",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280133523"
                    }
                ]
            },
            {
                "id": "bd3f350499760679e71b90ace7ad3e4974913c88",
                "type": "FIX",
                "text": " We no longer clear transition messages when a simple table refresh is done"
            },
            {
                "id": "4866e04fee9ab957dfe01e830a91fb550dd6aed1",
                "type": "FEATURE",
                "text": " we now support for semantic date range default values that require operators with parameters"
            },
            {
                "id": "bb65668e1d0109fcb45a8e91d0f6bd2daeb145d1",
                "type": "FIX",
                "text": " The title of an object page linked to a ConnectedFields is now displayed properly",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280140875"
                    }
                ]
            },
            {
                "id": "f50e99933f817f177d62180953b7f5f7afbf6cf0",
                "type": "FIX",
                "text": " We now provide some space between a table and a chart in analytical list pages",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280101233"
                    }
                ]
            },
            {
                "id": "398c6780cf98ccc406306885cbc9ad16477e79d0",
                "type": "FIX",
                "text": " We've resolved the issue related to adding a field to a form on a subobject page for key user adaptation"
            },
            {
                "id": "5c3563ffaa08f098550a79e127929f8a54beb5a6",
                "type": "FIX",
                "text": " A semantic link now works correctly even when a dynamic semantic object has unavailable actions",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280130100"
                    }
                ]
            }
        ]
    }
