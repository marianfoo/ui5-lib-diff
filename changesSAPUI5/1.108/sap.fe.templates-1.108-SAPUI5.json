        "notes": [
            {
                "id": "31071a26e46a9e03f852d5aa97455092038f45f0",
                "type": "FIX",
                "text": " We now ensure a list report with multiple views and a control variant is displayed correctly",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "002075129400000961772023"
                    }
                ]
            },
            {
                "id": "cf626d777f73835cf93630cb384b5b209c549edc",
                "type": "FIX",
                "text": " We now loop through all the filter conditions and then clear them instead of clearing only a single condition",
                "references": [
                    {
                        "type": "BCP",
