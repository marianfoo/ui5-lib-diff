{
    "1.106.0": {
        "date": "August 2022",
        "notes": [
            {
                "id": "badd74da40407ca76e642238012cbd7adae48d9e",
                "type": "FIX",
                "text": " A quick view now shows the correct data after displaying another quick view before",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280160441"
                    }
                ]
            },
            {
                "id": "a54bc214dcc2046e4586119d8cb424d94df8baed",
                "type": "FIX",
                "text": " The date is now properly formatted in a table group header"
            },
            {
                "id": "61eb0647b9bce633e9e413e74aa24652fb880119",
                "type": "FIX",
                "text": " We now avoid empty value helps if there is no internal binding context",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280161524"
                    }
                ]
            },
            {
                "id": "1d4ad90dd27ecfbfe7416c91cee14230b322c2f5",
                "type": "FEATURE",
                "text": " We now provide the possibility to use a default action handler for menu buttons in list reports, object page headers, and forms"
            },
            {
                "id": "fe4b802ed8cf1b05a8f7cd3d48f1e7baf2034a4c",
                "type": "FEATURE",
                "text": " We've added further features to the building blocks for tables and columns"
            },
            {
                "id": "b6e5b8bf7a3545d7fc083cc71ae639df3c9171eb",
                "type": "FEATURE",
                "text": " You can now display up to 6 columns in forms"
            },
            {
                "id": "567acb93a614d47f97ea5b7c55c2e65164e58ab6",
                "type": "FIX",
                "text": " The 'filter bar' building block is now generated properly if a metaPath is used that doesn't depend on the contextPath",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280140763"
                    }
                ]
            },
            {
                "id": "25597998e066ec3af2f1577a53cd53877324ed9b",
                "type": "FEATURE",
                "text": " We've enabled the handling of side effects for mass edit"
            },
            {
                "id": "159e4da54bc29d5941dfd630d600b88b14a9f039",
                "type": "FIX",
                "text": " The filter restrictions for the edm.Date property now work correctly",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280116999"
                    }
                ]
            },
            {
                "id": "36664b91787bfb00091e7e0c1427cab147c3e954",
                "type": "FIX",
                "text": " The UI.Hidden annotation now works correctly with reference facets pointing to another entity set via a navigation path",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280144773"
                    }
                ]
            },
            {
                "id": "c6deb36ec06087bf7ecfac6c3c4806a54dbb1fc6",
                "type": "FIX",
                "text": " SAP Fiori elements now supports low resolution on quick filters",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280144712"
                    }
                ]
            },
            {
                "id": "5111476b046b29368a9874994196cd29243ba1de",
                "type": "FIX",
                "text": " We've fixed an issue relating to side effects in combination with columns added via table settings",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280137459"
                    }
                ]
            },
            {
                "id": "1383b3599239dbf4bf51d2bfc12ecd6eac8b5310",
                "type": "FEATURE",
                "text": " SAP Fiori elements for OData V4 has replaced the deprecated AggregatedProperties with AggregatedProperty"
            },
            {
                "id": "70599a5448512b5b1244d2c65327d1e26f87d5b0",
                "type": "FIX",
                "text": " We now correctly apply SemanticObjectMappings when navigating via a SemanticObject that is dynamic",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280145046"
                    }
                ]
            },
            {
                "id": "55e2c4bd97381ffe310d50d6d77f305f3d65cbb0",
                "type": "FIX",
                "text": " We now remove the text arrangement property when #textOnly is used from the column's propertyInfo object",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280137173"
                    }
                ]
            },
            {
                "id": "bd3f350499760679e71b90ace7ad3e4974913c88",
                "type": "FIX",
                "text": " We no longer clear transition messages when a simple table refresh is done"
            },
            {
                "id": "eb3dab69e0b3a26613e7917b3d02f9777135d177",
                "type": "FIX",
                "text": " The content of a quick view facet with a navigation path in an analytical list page is now displayed correctly",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280135737"
                    }
                ]
            },
            {
                "id": "bb65668e1d0109fcb45a8e91d0f6bd2daeb145d1",
                "type": "FIX",
                "text": " The title of an object page linked to a ConnectedFields is now displayed properly",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280140875"
                    }
                ]
            },
            {
                "id": "398c6780cf98ccc406306885cbc9ad16477e79d0",
                "type": "FIX",
                "text": " We've resolved the issue related to adding a field to a form on a subobject page for key user adaptation"
            },
            {
                "id": "5c3563ffaa08f098550a79e127929f8a54beb5a6",
                "type": "FIX",
                "text": " A semantic link now works correctly even when a dynamic semantic object has unavailable actions",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280130100"
                    }
                ]
            },
            {
                "id": "42cf99ead92f67b457af71ab42f83420df6722b3",
                "type": "FIX",
                "text": " We now read default values of parameters of unbound actions"
            },
            {
                "id": "a50097e2ae8ad2219ee4278066ac2888c97b574b",
                "type": "FEATURE",
                "text": " We now use the new value help control in table and filter setting dialogs"
            },
            {
                "id": "da326cbd140bb26c0ef705336dd633bb90c15862",
                "type": "FIX",
                "text": " We now show the correct title in an action parameter dialog when the action label is undefined",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280141181"
                    }
                ]
            },
            {
                "id": "94666b50ba9bbdfed97786a87dfb5b63ee2af29c",
                "type": "FIX",
                "text": " The value state of mandatory filter fields now reacts to input sent via the ExtensionAPI.setFilterValues() method",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280110522"
                    }
                ]
            },
            {
                "id": "cd7a648fe28f05f5f26a977546a07a3853483e86",
                "type": "FEATURE",
                "text": " SAP Fiori elements now displays a DataPoint with a semantic object properly in the object page header"
            },
            {
                "id": "ab36774435d9292a5fd14b859104ce480dcdda5d",
                "type": "FEATURE",
                "text": " The type-ahead feature now highlights all entries beginning with your search string"
            },
            {
                "id": "351b1e248cc9d3a01fad6a793b1d29720c053b56",
                "type": "FIX",
                "text": " We now display the correct i18n text also in 'Delete' scenarios",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280115885"
                    }
                ]
            },
            {
                "id": "9167eb4b021bd9562bc34cdc7dd887d6555c84ca",
                "type": "FIX",
                "text": " SAP Fiori elements for OData V4 now automatically sets the height of charts to 100% in a chart-only view",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270018895"
                    }
                ]
            },
            {
                "id": "fbc9006b9637f6ce4adac40ffaa611929de03596",
                "type": "FIX",
                "text": " We now pass the correct component to the API of the CacheHandlerServiceFactory",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270021471"
                    }
                ]
            },
            {
                "id": "e9c57b4c4373771871a7b005ada7e186ad5666b5",
                "type": "FIX",
                "text": " When a semantic object is defined with a path, the unavailable actions of the semantic object now work properly",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280130100"
                    }
                ]
            },
            {
                "id": "135258c567b4cc4ffd785a6b3036d45e4a1fb570",
                "type": "FIX",
                "text": " SAP Fiori elements now enables you to filter on a table for navigation properties in the LineItem",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2170295934"
                    }
                ]
            },
            {
                "id": "b8d2001cd52553852eb23403d2ed9682edffaad4",
                "type": "FIX",
                "text": " You can now correctly save a variant (using 'Save View') after removing a column",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270103510"
                    }
                ]
            },
            {
                "id": "d7acbfa3f9d9a49d6a96ff32512eb84d404a1ec6",
                "type": "FIX",
                "text": " SAP Fiori elements for OData V4 now supports table visualization even if no lineItem annotations have been defined",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280114335"
                    }
                ]
            },
            {
                "id": "2e2e6ebe07f0cec98e0f747e08125f2de307ba9e",
                "type": "FEATURE",
                "text": " You can now mark custom filter fields as 'mandatory' by setting the 'required' property in the manifest.json"
            },
            {
                "id": "2f7b17d28f8cc2abca4c3243e5110ee2321fae01",
                "type": "FIX",
                "text": " The issue relating to the console error that occurred when adding a custom filter has been resolved",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280097031"
                    }
                ]
            },
            {
                "id": "447f60056411a6de21bc40daff3029f7eea7b443",
                "type": "FIX",
                "text": " The issue related to the calculation of the column width has been resolved",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280120627"
                    }
                ]
            },
            {
                "id": "b24faba35b74c43ac2741600a47c9f3dba83c29a",
                "type": "FIX",
                "text": " Customers can now provide their own custom texts for critical inline actions",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "002075129500004464442022"
                    }
                ]
            },
            {
                "id": "6322fa8424039962829200fb6a7a36fbfd1aad05",
                "type": "FIX",
                "text": " The issue related to the separator in the object page header has been resolved.",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280122403"
                    }
                ]
            },
            {
                "id": "7b470052795ff35c6661bcb3615e9d43886b7d3d",
                "type": "FEATURE",
                "text": " SAP Fiori elements for OData V4 now supports the TextArrangement annotation for the 'Group' label in responsive tables"
            },
            {
                "id": "3ea49925d22bbfc2b87830b50c0376345688f83a",
                "type": "FIX",
                "text": " Whenever a table is bound, the bindingUpdated is now properly fired",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270094029"
                    }
                ]
            }
        ]
    }
}