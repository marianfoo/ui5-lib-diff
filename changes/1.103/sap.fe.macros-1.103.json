{
    "1.103.0": {
        "date": "May 2022",
        "notes": [
            {
                "id": "1c785054a88ba0f060486fbe1934b05f43a00efb",
                "type": "FIX",
                "text": " Connected fields are now rendered properly in edit mode",
                "references": [
                    {
