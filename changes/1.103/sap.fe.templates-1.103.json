{
    "1.103.0": {
        "date": "May 2022",
        "notes": [
            {
                "id": "2b13dca20d74dcab75db6fbb9967cdbbf63355da",
                "type": "FIX",
                "text": " We've now enabled filtering on object page tables for DataFieldWithIntentBasedNavigation and DataFieldWithAction",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270075436"
                    }
                ]
            },
            {
                "id": "af73145766ab73b106949f78eab9fc5981fd6d84",
                "type": "FIX",
                "text": " We've removed the duplicate error messages that appeared when a user refreshed a table",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270097540"
                    }
                ]
            },
            {
                "id": "94ad1743f81c811891cd9f7f8a546c43cea5abcb",
                "type": "FIX",
                "text": " We now ensure users navigate back to the original app after deleting an object in another app where flexible column layout isn't enabled",
