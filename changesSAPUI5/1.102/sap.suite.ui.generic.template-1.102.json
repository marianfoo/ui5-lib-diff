            {
                "id": "I0a7b51e2541a273ec0a1feb7373111e8bfe307e7",
                "type": "FEATURE",
                "text": "Generic copy action is now implemented"
            },
            {
                "id": "I82e2b802c2dcc5eed128eb741df95848389a6f5b",
                "type": "FEATURE",
                "text": "Content-ID for preferred mode Edit has been introduced"
            },
            {
                "id": "I0f788ac81b2eaf2c41764688fcbdfeedc9e76143",
                "type": "FEATURE",
