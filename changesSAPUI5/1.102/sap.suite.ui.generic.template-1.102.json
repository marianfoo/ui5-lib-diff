            },
            {
                "id": "Ia8978621aad487d1066e97d045575e3f3495cfb8",
                "type": "FEATURE",
                "text": "Multiple empty table rows in edit mode are now supported"
            },
            {
                "id": "Icfe22c0540a266f5aa31d8d4ab934617da1a86bb",
