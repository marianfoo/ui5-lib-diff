{
    "1.100.0": {
        "date": "March 2022",
        "notes": [
            {
                "id": "765d953ef34b0de459e01e8010215b8cc4e40ffc",
                "type": "FIX",
                "text": " The DraftIndicator fragment now uses the correct formatter function",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "002075129400001432112022"
                    }
                ]
            },
            {
                "id": "3ec63b86d53030ac56d2a1dd023b05004ad06596",
                "type": "FEATURE",
                "text": " You can now use singletons to implement restrictions on tables on the object page"
            },
            {
                "id": "79bdb3b369f79c50e0951b7bd93f23dc4060e7f8",
                "type": "FEATURE",
                "text": " SAP Fiori elements now supports the annotations DataFieldWithIntentBasedNavigation and DataFieldWithAction"
            },
            {
                "id": "ea9aceeb9afa5190af6b543523003624e2e848de",
                "type": "FEATURE",
                "text": " SAP Fiori elements now displays a confirmation popup for actions that trigger a warning with response code 412"
            },
            {
                "id": "487800b846fe08c26a97c176dee0e9f54470dbd6",
                "type": "FEATURE",
                "text": " Switching to edit mode now checks if there's a side effect with a trigger action"
            },
            {
                "id": "e60e781063a9e5fa862138d945b149106ebcbad2",
                "type": "FIX",
                "text": " The message popup no longer opens in a message page",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280041541"
                    }
                ]
            },
            {
                "id": "92dd9d5feea8a32176ac472eaa6e50be8915e9bc",
                "type": "FEATURE",
                "text": " SAP Fiori elements now supports the 'Importance' property for custom columns"
            },
            {
                "id": "685bd394d4145d5eaf2067d8e3b3cd29f46b90a5",
                "type": "FIX",
                "text": " We ensured, that the message strip for ignored filter fields on the list report also works correctly if the text to be displayed contains apostrophs",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280049603"
                    }
                ]
            },
            {
                "id": "1ce9886cfb6fc96ec2229adeb54047607859ba73",
                "type": "FIX",
                "text": " The Create dialog for immutable fields now works correctly in case of errors",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280047255"
                    }
                ]
            },
            {
                "id": "a1b2335e3ba5f4373705fb1e3f4b360dbd108cf4",
                "type": "FIX",
                "text": ": SAP Fiori elements now correctly displays the selected tab in the page VariantManagement for list reports using multiple table mode"
            },
            {
                "id": "7ece58065bf0216452464baab2d12c6dd6296de1",
                "type": "FEATURE",
                "text": " You can now hide the filter bar on a list report to enable a worklist"
            },
            {
                "id": "1da99a3b44e03de3e5c29bcdddf05b616a4aab2e",
                "type": "FIX",
                "text": " SAP Fiori elements now correctly displays the currency inside a DataPoint",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280039812"
                    }
                ]
            },
            {
                "id": "5963b77c74df91168d1a321649cb8437b5491536",
                "type": "FEATURE",
                "text": " The PropertyName within a SelectionOptions property of the SelectionVariant UI annotation can now be defined with a navigation property"
            },
            {
                "id": "21e29b3214f9d3b061a6adde7847f89d9f85b62d",
                "type": "FEATURE",
                "text": " You can now define a ValueListForValidation annotation with a conditional expression binding"
            },
            {
                "id": "ffde8e9d121504b59aafb634fb6dcdceb192f427",
                "type": "FEATURE",
                "text": " In sticky mode, custom \"create\" actions now result in the object page opening in edit mode"
            },
            {
                "id": "5bb69e45d8165a33a575ff9322cd59fb51b7eb92",
                "type": "FIX",
                "text": " A field with a currency in a form is now properly aligned with its unit",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280016943"
                    }
                ]
            },
            {
                "id": "1be7c895f6644c2d24eafcec50f18221155f3a00",
                "type": "FEATURE",
                "text": " You can now display MultiValueField in Tables for a DataField that uses a 1:n navigation in the value path"
            },
            {
                "id": "28c75a5c1d84c7f47ae9f4b6908c5d17cb371716",
                "type": "FEATURE",
                "text": " The 'Field' building block now also supports the length definition for MultiLine text fields"
            },
            {
                "id": "c4ebf38c75cb9e984a4500672349d7e036c0a34a",
                "type": "FIX",
                "text": " SAP Fiori elements now correctly displays the avatar based on the HeaderInfo",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2280031580"
                    }
                ]
            },
            {
                "id": "6dfc689bc98b201e6a2b1a8df11b4f0b4b14f053",
                "type": "FIX",
                "text": " Error handling is now included for a chain of promises when a user saves a document",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "002075129400000390322022"
                    }
                ]
            },
            {
                "id": "7601e161178fe51eb0cf27bf54bb99074f17eea8",
                "type": "FIX",
                "text": " We have removed the duplicate 503 error message for the patch service",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2170327750"
                    }
                ]
            },
            {
                "id": "1d9aae326a0ddee22d3ba1aa21a92c2277c2d598",
                "type": "FEATURE",
                "text": " The filter bar is now hidden after a user presses the Go button on a mobile phone"
            },
            {
                "id": "7e7d3d6e6e3c8f9b20e68d2035dbf9e25c82ff65",
                "type": "FEATURE",
                "text": " SAP Fiori elements now provides the snappedTitleOnMobile for object pages used on a mobile phone"
            },
            {
                "id": "a3f4fce387a2062600f995997a54fbfb87c87ec2",
                "type": "FIX",
                "text": " A browser console error that occurred on the title hierarchy has been fixed",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2180423604"
                    }
                ]
            },
            {
                "id": "86d32cec4b45e39ee210c6ae5d47663845a07af1",
                "type": "FEATURE",
                "text": " You can now use the search field in charts in the list report and analytical list page"
            }
        ]
    }
}