                    }
                ]
            },
            {
                "id": "68aff68a4f82f4e7a9b61fa9cdad32b0bd61451f",
                "type": "FIX",
                "text": " We now trigger a new GET request whenever a user clicks the Go button in a filter bar of a value help dialog",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270102660"
                    }
                ]
            },
            {
                "id": "2b0e7283e3036c18467e034be76aa196e3123322",
                "type": "FIX",
                "text": ": We've adapted the settings for actions in the manifest.json to consider actions in the Menu button",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2270104424"
                    }
                ]
            },
            {
                "id": "19fc756af16cdb7f6042bc62cba485eba117d7b4",
