{
    "1.120.0": {
        "date": "October 2023",
        "notes": [
            {
                "id": "cce560a98b5c822be70fcd332f76b2161df5e01e",
                "type": "FIX",
                "text": " SideEffects network requests on structural changes (create/delete) relying on MultiValueField are now ordered appropriately",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370113871"
                    }
                ]
            },
            {
                "id": "9fdde7b62ca9960259eaf0d9f8330d81eba5a327",
                "type": "FIX",
                "text": " We now ensure that the columns containing units are correctly exported",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "002075129500008126452023"
                    }
                ]
            },
            {
                "id": "faa78a46383d8c1f33a2992b989a9aa62bd73853",
                "type": "FIX",
                "text": " We've resolved an issue with the navigation from a quick view when the TextArrangementType annotation is used for the field",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370108954"
                    }
                ]
            },
            {
                "id": "76e291599045ad927e481a489ee2ef21de4e233d",
                "type": "FIX",
                "text": " We now ensure that the draft indicator is not templated in key columns inside grid tables",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370115804"
                    }
                ]
            },
            {
                "id": "c84b4502dd116e6aaff9b6aca2c36e9a189fca6f",
                "type": "FIX",
                "text": " We now ensure that the header level for the object page table and macro chart are set correctly",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370094825"
                    }
                ]
            },
            {
                "id": "72261e83920cea7ad948cd6b96a03c372ea920fd",
                "type": "FIX",
                "text": " We've resolved an issue with the column width calculation when the table is hidden",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370110305"
                    }
                ]
            },
            {
                "id": "a87192db7b5d1db2a5efeb9d9d30601f1b6fa09a",
                "type": "FIX",
                "text": " We've resolved an issue with the quick view not appearing when the user clicks on fields and field links",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370114281"
                    }
                ]
            },
            {
                "id": "2c563630ca92d277493b088df0e4495fa23ed86d",
                "type": "FIX",
                "text": " We now ensure that the avatar of the user locking the object in a collaborative draft scenario is resized to fit correctly",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370098192"
                    }
                ]
            },
            {
                "id": "6edb77af072ffbbe657cd6799b29a3a0ae431ce2",
                "type": "FIX",
                "text": " We’ve resolved an issue with duplicate fields in contact cards",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370110068"
                    }
                ]
            },
            {
                "id": "bf990021dc8064eca3e2ca5619963cbf32994967",
                "type": "FIX",
                "text": " We now ensure that the metadata model of the control is linked to the value help for conditions",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370101862"
                    }
                ]
            },
            {
                "id": "5918983fc72e7b05246579da49922c8c401a240e",
                "type": "FEATURE",
                "text": " We now display separate columns for the editing status and business situation in grid tables"
            },
            {
                "id": "f9cad4ac011c48e559e24b7470cbc8bd20bd19a1",
                "type": "FIX",
                "text": " We now show the empty indicator if a contact block link is not maintained",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370048588"
                    }
                ]
            },
            {
                "id": "33e1f48149e2eee4cd010798b31f956e007d3165",
                "type": "FIX",
                "text": " We now provide the correct formatting and text arrangement for semantic key fields annotated with semantic objects",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370089743"
                    },
                    {
                        "type": "BCP",
                        "reference": "2370086614"
                    }
                ]
            },
            {
                "id": "5f07ce0a0857162c1554f0900ea21074beef394a",
                "type": "FEATURE",
                "text": " You can now use the communication options provided by Microsoft Teams in the quick view containing the contact information"
            },
            {
                "id": "844a4474541f323f73018d50a2e085f7fd4bc520",
                "type": "FIX",
                "text": " We’ve resolved an issue that resulted in the contact card automatically closing in some cases",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370096409"
                    }
                ]
            },
            {
                "id": "3437b1702a16d09bc69394ae091a8c2d6d29f3b6",
                "type": "FIX",
                "text": " We now ensure that the secondary actions in the page-level toolbar appear in ghost style and the secondary actions in the content-level toolbar appear in transparent style.",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370067990"
                    }
                ]
            },
            {
                "id": "31406594569db584e5d57c8080fbbe1e2d0ca350",
                "type": "FIX",
                "text": " We now display the avatar in the correct aspect ratio within the collapsed header",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370099636"
                    }
                ]
            },
            {
                "id": "6a5b50ae69eef4c397ef5667914e2b271a0d0beb",
                "type": "FIX",
                "text": " We now ensure that the columns containing timestamps are correctly exported",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370097686"
                    }
                ]
            },
            {
                "id": "a37cf42a59e703404e3ee9616f21f52b9afd521a",
                "type": "FEATURE",
                "text": " SAP Fiori elements for OData V4 now allows you to use the KPITag building block in list reports"
            },
            {
                "id": "df8e37e6a86f2cda75b0f356ec77512641957558",
                "type": "FEATURE",
                "text": " You can now define the table selection mode using the parameters ForceMulti and ForceSingle, even when no actions are available"
            },
            {
                "id": "9684125875e407ceb28b45604af2a6acd68dea75",
                "type": "FEATURE",
                "text": " When you click on the email field used in a table or a form, you can now use the communication options provided by Microsoft Teams"
            },
            {
                "id": "77271c571a2afd09f57b17a2006b444b9a9ad16f",
                "type": "FIX",
                "text": " We now evaluate the HeaderInfo.TypeNamePlural annotation as an empty string if it isn't defined",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2380101495"
                    }
                ]
            },
            {
                "id": "e6043f3106c5ec53dc98a08dfec2c24105457ad7",
                "type": "FEATURE",
                "text": " We now allow the MultiValueField to be used in edit mode within a table"
            },
            {
                "id": "3c8e13b4e82535ac6dbbb4068928455d87aa155e",
                "type": "FIX",
                "text": " We've added sap.fe.macros.FlexibleColumnLayoutActions to the API documentation",
                "references": [
                    {
                        "type": "BCP",
                        "reference": ""
                    }
                ]
            },
            {
                "id": "a35e1b82da00512fa6ccb9333294f86d9b0bcdbb",
                "type": "FIX",
                "text": " We have resolved an issue with the custom configuration of the MenuButton",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370088594"
                    }
                ]
            },
            {
                "id": "11b30513f8154115c3e2dacf615131d4f5ad0f19",
                "type": "FIX",
                "text": " We no longer show an incorrect property '$editState' in the table personalization dialog",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370100245"
                    }
                ]
            },
            {
                "id": "391d07e8270a8659c3e688e7f4a821596b281227",
                "type": "FIX",
                "text": " We have resolved several issues with using inline creation mode in building block tables",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2380116651"
                    }
                ]
            },
            {
                "id": "b7f6e83b83ed0f6babd400503f3f5c4641b9ac64",
                "type": "FIX",
                "text": " We’ve resolved an issue with navigating to an incorrect TargetControl from the message button",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370085117"
                    }
                ]
            },
            {
                "id": "17850052ae29557a5dfcc8d3f28b378546279a67",
                "type": "FIX",
                "text": " We now ensure correct formatting and text arrangement for semantic key fields annotated with semantic objects",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370089743"
                    },
                    {
                        "type": "BCP",
                        "reference": "2370086614"
                    }
                ]
            }
        ]
    },
    "1.120.1": {
        "date": "December 2023",
        "notes": [
            {
                "id": "09cacf18660ba301f8e30dfcefea86a4866d424e",
                "type": "FIX",
                "text": " We now ensure that a multi-valued field in a list report that uses the flexible column layout is not editable when the object page is in edit mode",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2380135952"
                    }
                ]
            },
            {
                "id": "abc2eb3ff71bc2cd4cac2cf9256f7dbdbdcf837a",
                "type": "FIX",
                "text": " We now check for the maximum allowed length of an email address when entering it in an input field",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2380136662"
                    }
                ]
            },
            {
                "id": "e39b8d0a96b3d61ed89a6d66b84a9d02858d7da8",
                "type": "FEATURE",
                "text": " We now recalculate the default values in empty row mode if the table is refreshed using a side effect"
            },
            {
                "id": "93110b32fcb484a4d586c6ee39b421249b170056",
                "type": "FIX",
                "text": " We now ensure that the relevant custom filters are also correctly applied to other entity sets",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370128365"
                    }
                ]
            },
            {
                "id": "46b50de2ce2a6b9aef8a597ab8aa0a0a2429869e",
                "type": "FIX",
                "text": " We've now added the prefix 'cmd:' to the commands defined in the manifest for actions",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370144348"
                    }
                ]
            },
            {
                "id": "d1daf47e7e055cf59eb35776282793d59720b4a0",
                "type": "FIX",
                "text": " We now ensure that the byID provided by our ExtensionAPI returns custom controls",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370136106"
                    }
                ]
            },
            {
                "id": "15b18b82680783e07af1ef0fc562127884f56bb6",
                "type": "FIX",
                "text": " We now ensure text annotation is considered for semantic keys inside a grid table",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370146239"
                    }
                ]
            },
            {
                "id": "ecfac65e08ea0da3095abdbf61ed59d60f923135",
                "type": "FIX",
                "text": " We now ensure that the columns containing progress data points are correctly exported",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370100303"
                    }
                ]
            },
            {
                "id": "b4b75fac9febb26909e2e8e1b116370e35ff5f6f",
                "type": "FIX",
                "text": " We've resolved an issue with the width of a text area in a table",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370137269"
                    }
                ]
            },
            {
                "id": "5f6dc32ca2d809cfa8a60b89d9543edb80f3bb72",
                "type": "FIX",
                "text": " We now correctly manage the SelectionVariant annotation configured with the RangeSignType ‘Exclude’ for multiple views",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "002075129400007247152023"
                    }
                ]
            },
            {
                "id": "42ab62d2d6e3169c87e7d20131b35b6c907cba94",
                "type": "FIX",
                "text": " We’ve resolved an issue that prevented updating a field with the static unit of measure ‘%’",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370134566"
                    }
                ]
            },
            {
                "id": "8318620c5c9b248b78883012648d4a55b91dba73",
                "type": "FIX",
                "text": " Drag and drop is now disabled when dragging an item from a tree table and dropping it outside the tree table",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370131643"
                    }
                ]
            },
            {
                "id": "a656ede05febeb6f5f698e8b97581fb624db630e",
                "type": "FIX",
                "text": " When exporting a table to spreadsheet, the \"Target Value\" column is no longer included for DataPoints without a target value",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "002075129500008815062023"
                    }
                ]
            },
            {
                "id": "afa8985859143f924c26bdaa65ddc948b8e4da79",
                "type": "FIX",
                "text": " The messageButton link is now disabled if the associated message is not targeting a property",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370106811"
                    }
                ]
            },
            {
                "id": "2c8486e82981ac422c4949996f08ad2fdc613a6f",
                "type": "FIX",
                "text": " We have resolved several issues when adding items using a MultiValueField",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370121682"
                    }
                ]
            },
            {
                "id": "d1c5402311de88b58fcfd385f1a0c3542e83a259",
                "type": "FIX",
                "text": " We now ensure the filter values of value help used on a multi-valued field are retrieved correctly",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370107271"
                    }
                ]
            }
        ]
    },
    "1.120.2": {
        "date": "January 2024",
        "notes": [
            {
                "id": "6d1671178ca532fd269f8c4386c94b4e2326e4dd",
                "type": "FIX",
                "text": " The Form building block can now have a metaPath that uses 1:1 navigation pointing to a FieldGroup annotation"
            },
            {
                "id": "becd199bd227de7abcd5d298d12ebbf687168b03",
                "type": "FIX",
                "text": " We now ensure a MultiValue Field that’s used in a read-only Table building block isn’t editable",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370150811"
                    }
                ]
            }
        ]
    },
    "1.120.3": {
        "date": "January 2024",
        "notes": [
            {
                "id": "ab337cd42fd67722f5575a531bfdfcc31046cd61",
                "type": "FIX",
                "text": " You can now use the odata.concat function for labels in reference facets"
            },
            {
                "id": "0becef9b991c52c408c3f37a32b8ff02c31273e4",
                "type": "FIX",
                "text": " Filterable properties listed in table personalization are now synchronized with the filterable properties in the filter bar"
            }
        ]
    },
    "1.120.4": {
        "date": "February 2024",
        "notes": [
            {
                "id": "9dc6a39523f471851412cdfd3339dd487c9cfb86",
                "type": "FIX",
                "text": " We've resolved an issue that prevented some fields from being editable in an app using the flexible column layout and a sticky edit session.",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2370083543"
                    },
                    {
                        "type": "BCP",
                        "reference": "2370083469"
                    }
                ]
            },
            {
                "id": "7df22ada4476de28652dc9b32c819a4d558778bf",
                "type": "FIX",
                "text": " We no longer display ‘undefined’ under a collection facet when no label is defined"
            }
        ]
    },
    "1.120.6": {
        "date": "February 2024",
        "notes": [
            {
                "id": "fb17a38283b1bfe67e9933a64b4a3aef9b217987",
                "type": "FIX",
                "text": " We've resolved an issue with displaying the text annotation in the quick view title"
            }
        ]
    },
    "1.120.9": {
        "date": "April 2024",
        "notes": [
            {
                "id": "14aecd03be94c1a6c5ee6cad7e84f15410d5a884",
                "type": "FIX",
                "text": " We now clear the selection in a table after applying a search or a filter, except for responsive tables"
            },
            {
                "id": "a877cfd7a5aa49586fd613d03b17c505d4b411cf",
                "type": "FIX",
                "text": " We now align the column correctly when the table entity has no key"
            },
            {
                "id": "d8baaa37592aef3c80e94baa3dbe3abdc8109696",
                "type": "FIX",
                "text": " We now clear the messageTargetProperty to ensure the navigation works correctly after saving changes on the subobject page"
            },
            {
                "id": "98b0d6d5003c99ebc98f4f10a239d9c0c60551b9",
                "type": "FEATURE",
                "text": " Applications can now dynamically display the ‘Summarize’ button if the prerequisites are fulfilled"
            }
        ]
    }
}