                    {
                        "type": "BCP",
                        "reference": "2380068762"
                    }
                ]
            },
            {
                "id": "6b103c73a544bcecf9b3caa8856f126e40d6c727",
                "type": "FIX",
                "text": " We now ensure that xAppState is applied when the UI.SelectionFields isn't configured in the manifest",
                "references": [
                    {
                        "type": "BCP",
                        "reference": "2380066297"
                    }
                ]
            },
            {
                "id": "870e7c9863f487601e9b0d5fcf2546ed28722510",
